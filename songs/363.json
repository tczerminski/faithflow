{
    "id": 363,
    "title": "ODDAJMY DZIŚ",
    "verses": [
        {
            "lines": [
                "ODDAJMY DZIŚ",
                "WIECZNEMU BOGU CZEŚĆ",
                "NIECH NASZE SERCA WIELBIĄ GO",
                "ON STWORZYŁ NIEBIOSA,",
                "ON UTWIERDZIŁ ZIEMI KRĄG,",
                "JEGO CHWAŁA",
                "CAŁE NIEBO WYPEŁNIA WCIĄŻ,",
                "JEGO WIELKOŚĆ",
                "NA CAŁEJ ZIEMI OBJAWIONA JEST!"
            ]
        },
        {
            "lines": [
                "TO JEST NASZ BÓG!",
                "NASZ JEDYNY PAN!"
            ]
        }
    ]
}