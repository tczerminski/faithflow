    "title": "KIEDY PRZYCHODZISZ TY",
    "verses": [
        {
            "lines": [
                "KIEDY PRZYCHODZISZ TY,",
                "NIEBO NADE MNĄ OTWIERASZ",
                "KIEDY PRZYCHODZISZ TU,",
                "MĄ SŁABOŚĆ W MOC SWOJĄ ZAMIENIASZ"
            ]
        },
        {
            "lines": [
                "TWE RĘCE NIE SĄ ZBYT KRÓTKIE,",
                "BY PODNIEŚĆ MNIE",
                "ROZPOZNASZ WSZĘDZIE BOŻE WOŁANIE ME",
                "TWE OCZY ZAWSZE W TŁUMIE DOSTRZEGĄ MNIE",
                "BO CIĄGLE JESTEŚ PRZY MNIE, PANIE"
            ]
        },
        {
            "lines": [
                "TWE RĘCE NIE SĄ ZBYT KRÓTKIE,",
