{
    "id": 591,
    "title": "W BOGU UFNOŚĆ MAM",
    "verses": [
        {
            "lines": [
                "PODCZAS BURZ NIE PODDAM SIĘ",
                "W CZASIE BITWY WYZNAM, ŻE",
                "TWA ŁASKA STARCZY MI",
                "TWA ŁASKA STARCZY MI"
            ]
        },
        {
            "lines": [
                "CHOCIAŻ WALKA CIĄGLE TRWA",
                "NIGDY NIE OPUSZCZASZ NAS",
                "TY ZAWSZE STARCZASZ NAM",
                "TY ZAWSZE STARCZASZ NAM"
            ]
        },
        {
            "lines": [
                "W BOGU UFNOŚĆ MAM, W NIM NADZIEJA JEST",
                "JA WIEM, BÓG SIĘ NIE ZACHWIEJE",
                "BÓG JEST Z NAMI TU, ON ZWYCIĘŻYŁ JUŻ",
                "JA WIEM, BÓG SIĘ NIE ZACHWIEJE"
            ]
        },
        {
            "lines": [
                "MY PÓJDZIEMY TAM, GDZIE TY",
                "ZAUFAMY SŁOWOM TWYM",
                "TY POPROWADZISZ NAS",
                "TY POPROWADZISZ NAS"
            ]
        },
        {
            "lines": [
                "KIERUJ DROGĄ NASZYCH SERC",
                "MY NIESIEMY IMIĘ TWE",
                "TY NIE ZAWIEDZIESZ NAS",
                "TY NIE ZAWIEDZIESZ NAS"
            ]
        },
        {
            "lines": [
                "TY NIGDY NIE ZMIENIASZ SIĘ",
                "PRAWDZIWA NADZIEJA JEST W MIŁOŚCI TWEJ",
                "TY ZA NAS WALCZYSZ SAM I NIE MUSIMY SIĘ BAĆ",
                "BO KOCHASZ NAS"
            ]
        }
    ]
}