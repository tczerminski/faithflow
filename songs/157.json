{
    "id": 157,
    "title": "IMIĘ JEZUS",
    "verses": [
        {
            "lines": [
                "NIE MA IMIENIA NAD IMIĘ JEZUS",
                "TYLKO TO IMIĘ TAK PIĘKNE JEST",
                "NIE MA IMIENIA NAD IMIĘ JEZUS"
            ]
        },
        {
            "lines": [
                "JEZUS JEST ŚWIATŁEM",
                "CO ROZJAŚNIA MROK",
                "JEZUS JEST ŻYCIEM",
                "DLA DUSZY I SERCA",
                "ON JEST NADZIEJĄ",
                "MIŁOŚCIĄ I MOCĄ",
                "JEDYNĄ DROGĄ I PRAWDĄ JEST ON"
            ]
        }
    ]
}