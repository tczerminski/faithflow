{
    "id": 500,
    "title": "ŚPIEWAĆ CHCĘ TAK JAK MOJŻESZ",
    "verses": [
        {
            "lines": [
                "ŚPIEWAĆ CHCĘ TAK JAK MOJŻESZ",
                "BOGU MEMU PIEŚŃ",
                "I BARANKOWI",
                "STOJĄC PRZED CHWAŁY TRONEM",
                "SŁAWIĘ DROGI TWE",
                "WSPANIAŁOŚĆ TWOICH DZIEŁ"
            ]
        },
        {
            "lines": [
                "W DARZE CI DZIŚ PRZYNOSZĘ",
                "OWOC MOICH WARG,",
                "W OFIERZE SKŁADAM ŻYCIE ME",
                "TYLKO TY JESTEŚ BOGIEM",
                "Z BOJAŹNIĄ I CZCIĄ",
                "UWIELBIAM ŚWIĘTE IMIĘ TWE"
            ]
        },
        {
            "lines": [
                "ALLELUJA, O, ALLELUJA"
            ]
        }
    ]
}