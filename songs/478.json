{
    "id": 478,
    "title": "SŁAWIĘ PANIE IMIĘ TWE",
    "verses": [
        {
            "lines": [
                "SŁAWIĘ PANIE IMIĘ TWE,",
                "WZNOSZĘ RĘCE W UWIELBIENIU",
                "CIESZĘ SIĘ, ŻE JESTEŚ TU",
                "RADUJĘ SIĘ Z TWEGO ZBAWIENIA"
            ]
        },
        {
            "lines": [
                "Z NIEBA NA ZIEMIĘ TYŚ ZSZEDŁ",
                "BY DROGĄ BYĆ,",
                "POSZEDŁEŚ ZA MNIE NA KRZYŻ",
                "MÓJ SPŁACIĆ DŁUG",
                "NIE POWSTRZYMAŁ CIEBIE GRÓB,",
                "NIEBO ZNÓW UJRZAŁO CIĘ",
                "SŁAWIĘ PANIE IMIĘ TWE"
            ]
        }
    ]
}