{
    "id": 217,
    "title": "JEZUS JEST MYM PANEM, JEZUS ZNALAZŁ MNIE",
    "verses": [
        {
            "lines": [
                "JEZUS JEST MYM PANEM,",
                "JEZUS ZNALAZŁ MNIE.",
                "ON MNIE WYRWAŁ Z TEGO ŚWIATA,",
                "BYM MU SŁUŻYĆ MÓGŁ."
            ]
        },
        {
            "lines": [
                "(: TAK, TAK, TAK, O PANIE MÓJ,",
                "DZIĘKI, DZIĘKI CI :)"
            ]
        },
        {
            "lines": [
                "ON MI PODAŁ RĘKĘ",
                "I WYBACZYŁ TO,",
                "CO MNIE WIODŁO NA BEZDROŻA",
                "I PRZEPAŚCI DNO."
            ]
        },
        {
            "lines": [
                "WIDZĘ PIĘKNOŚĆ ŚWIATA,",
                "CZUJĘ MIŁOŚĆ TĄ,",
                "KTÓRĄ JEZUS MNIE OBDARZA,",
                "WIDZĄC SŁABOŚĆ MĄ."
            ]
        },
        {
            "lines": [
                "A JEŻELI PRAGNIESZ,",
                "UWOLNIONYM BYĆ,",
                "DOŁĄCZ GŁOS SWÓJ DO MOJEGO,",
                "ŚPIEWAJ ZE MNĄ PIEŚŃ."
            ]
        }
    ]
}