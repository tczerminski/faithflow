{
    "id": 322,
    "title": "NIEBO I ZIEMIA",
    "verses": [
        {
            "lines": [
                "NIEBO I ZIEMIA GWIAZDEK TYSIĄCE",
                "SREBRZYSTY KSIĘŻYC I ZŁOTE SŁOŃCE",
                "WSZYSTKO POWSTAŁO WRĘCZ WYSTRZAŁOWO",
                "NIE POPRZEZ WYBUCH LECZ BOGA SŁOWO"
            ]
        },
        {
            "lines": [
                "RYBY I PTAKI WIELKIE POTWORY",
                "I WSZYSTKIE INNE RUCHLIWE STWORY",
                "BOŻA KREACJA TO REWOLUCJA",
                "ALE NA PEWNO NIE EWOLUCJA"
            ]
        },
        {
            "lines": [
                "(: TO PAN BÓG STWORZYŁ TEN PIĘKNY ŚWIAT",
                "I ZDECYDOWAŁ JAK DZIAŁAĆ MA :)"
            ]
        },
        {
            "lines": [
                "(: DZIĘKI CI BOŻE ZA PORY ROKU",
                "KOLORY TĘCZY I SMAK OWOCÓW",
                "DZIĘKI ZA ŚWIAT KTÓRY ZACHWYCA NAS",
                "I ŻE NAD WSZYSTKIM KONTROLĘ MASZ :)"
            ]
        },
        {
            "lines": [
                "MAŁPA JEST EXTRA JEST SUPER ZWINNA",
                "NIE JEST TO JEDNAK MOJA KUZYNKA",
                "CZŁOWIEKA PAN BÓG SAM UKSZTAŁTOWAŁ",
                "I TCHNIENIEM ŻYCIA GO DOŁADOWAŁ"
            ]
        }
    ]
}