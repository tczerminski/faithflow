{
    "id": 460,
    "title": "RADUJ SIĘ Z NAMI, ZAŚPIEWAJ PIEŚŃ",
    "verses": [
        {
            "lines": [
                "RADUJ SIĘ Z NAMI ZAŚPIEWAJ PIEŚŃ",
                "KRZYCZ – ALLELUJA – RĘCE SWE WZNIEŚ",
                "JEŚLI CHCESZ SKAKAĆ – SKACZ ILE SIŁ",
                "WSZYSTKO PRZED PANEM",
                "ON GODZIEN JEST CZCI!"
            ]
        },
        {
            "lines": [
                "ON WYWIÓDŁ MNIE Z CIEMNOŚCI",
                "ZWYCIĘŻYŁ ŚMIERĆ I GRZECH",
                "BO W BLASKU TEJ ŚWIATŁOŚCI",
                "ZŁO NIE OSTOI SIĘ",
                "ON DOTKNĄŁ MEGO SERCA",
                "I PRZEJĄŁ JEGO RYTM",
                "OCZYŚCIŁ JE DO KOŃCA",
                "MNIE NAZWAŁ DZIECKIEM SWYM!"
            ]
        },
        {
            "lines": [
                "ON KRÓLEM CAŁEJ ZIEMI",
                "OBIECAŁ PO NAS PRZYJŚĆ",
                "WIĘC NIE PRZESTAŃMY WIERZYĆ",
                "ŻE Z NIM BĘDZIEMY ŻYĆ!",
                "JAKO JEDYNY CZŁOWIEK",
                "POKONAŁ ŚMIERCI GRZECH",
                "WIĘC DZISIAJ Z NIM ŻYJEMY",
                "ODDAJMY JEMU CZEŚĆ!"
            ]
        }
    ]
}