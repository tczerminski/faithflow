{
    "id": 280,
    "title": "MÓJ PAN MNIE DOTKNĄŁ (BYŁEM ZNIEWOLONY)",
    "verses": [
        {
            "lines": [
                "BYŁEM ZNIEWOLONY GRZECHEM,",
                "CIĘŻAR WIN PRZYTŁACZAŁ MNIE.",
                "JEZUS RĘKĄ SWĄ MNIE DOTKNĄŁ",
                "I JUŻ NIE JESTEM TAKI SAM."
            ]
        },
        {
            "lines": [
                "MÓJ PAN MNIE DOTKNĄŁ, ON MNIE DOTKNĄŁ",
                "TO SPRAWIA RADOŚĆ W SERCU MYM.",
                "DUCH TWÓJ OGARNIA, PRZENIKA MNIE",
                "I ZMIENIA NA OBRAZ TWÓJ."
            ]
        },
        {
            "lines": [
                "ODKĄD MNIE MÓJ ZBAWCA ZNALAZŁ",
                "I OCZYŚCIŁ Z WSZYSTKICH WIN,",
                "NIE PRZESTANĘ GO UWIELBIAĆ",
                "AŻ DO KOŃCA MOICH DNI."
            ]
        }
    ]
}