{
    "id": 41,
    "title": "BÓG JEST SCHRONIENIEM",
    "verses": [
        {
            "lines": [
                "BÓG JEST SCHRONIENIEM I SIŁĄ MĄ",
                "POMOCĄ W KAŻDEJ CHWILI ZŁEJ",
                "ON TWIERDZĄ I WYBAWIENIEM JEST",
                "PRAWDZIWYM PRZYJACIELEM MYM"
            ]
        },
        {
            "lines": [
                "TYŚ MYM PASTERZEM NIE ZBŁĄDZĘ JUŻ",
                "W POKOJU UKŁADASZ MNIE SAM",
                "DO ŹRÓDEŁ SPOKOJNYCH WÓD",
                "PROWADZISZ MNIE",
                "ODŻYWA MÓJ DUCH U TWYCH STÓP"
            ]
        },
        {
            "lines": [
                "A JEŚLI CIEMNĄ DOLINĄ BYM SZEDŁ",
                "NICZEGO NIE BĘDĘ SIĘ BAŁ",
                "TWA LASKA I KIJ TWÓJ POCIESZA MNIE",
                "WIEM, ŻE NIGDY NIE BĘDĘ JUŻ SAM"
            ]
        }
    ]
}