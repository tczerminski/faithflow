{
    "id": 10,
    "title": "ANI TY, ANI JA, ANI NAWET CAŁY ŚWIAT",
    "verses": [
        {
            "lines": [
                "ANI TY, ANI JA,",
                "ANI NAWET CAŁY ŚWIAT,",
                "ANI RĘKA, ANI NOGA,",
                "NIE POTRAFIĄ ŻYĆ BEZ BOGA"
            ]
        },
        {
            "lines": [
                "POTRZEBUJĘ CIĘ",
                "TY PRZEBACZASZ MI MÓJ GRZECH",
                "POTRZEBUJĘ CIĘ",
                "TY OCZYSZCZASZ SERCE ME",
                "POTRZEBUJĘ CIĘ",
                "I NA DOBRE I NA ZŁE",
                "POTRZEBUJĘ CIĘ",
                "PANIE JEZU RATUJ MNIE"
            ]
        }
    ]
}