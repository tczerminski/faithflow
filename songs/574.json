    "id": 574,
    "title": "TYŚ JEST SCHRONIENIEM MYM",
    "verses": [
        {
            "lines": [
                "TYŚ JEST SCHRONIENIEM MYM",
                "WYPEŁNIASZ SERCE ME",
                "TY DAJESZ WYZWOLENIA PIEŚŃ",
                "I JUŻ NIE BOJĘ SIĘ",
                "BO ZAWSZE UFAM CI, ZAWSZE UFAM CI",
                "WIĘC W SŁABOŚCI MEJ WYZNAĆ CHCĘ",
                "W MOCY TWEJ MOJA MOC"
            ]
        }
    ]
}