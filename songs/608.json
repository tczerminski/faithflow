{
    "id": 608,
    "title": "WEDŁUG ŁASKI BOŻEJ",
    "verses": [
        {
            "lines": [
                "WEDŁUG ŁASKI BOŻEJ",
                "KTÓRA JEST MI DANA",
                "WYBUDUJĘ DOM NA SKALE",
                "W MYŚL MOJEGO PANA",
                "JEŚLI DZIEŁO MOJE SPŁONIE",
                "SAM PONIOSĘ SZKODĘ",
                "A JEŚLI SIĘ OSTOI",
                "OTRZYMAM NAGRODĘ"
            ]
        },
        {
            "lines": [
                "MOŻE TO JEST SIANO (O NIE)",
                "MOŻE TO JEST TYLKO SŁOMA (O NIE)",
                "MOŻE TO KAMIENIE (O NIE)"
            ]
        },
        {
            "lines": [
                "POZNASZ PRAWDĘ W SĄDU WIELKI DZIEŃ",
                "OGIEŃ WYPRÓBUJE DZIEŁO TWE",
                "CZY BUDOWLA TWA OSTOI SIĘ"
            ]
        }
    ]
}