{
    "id": 465,
    "title": "ROZPAL DZIŚ",
    "verses": [
        {
            "lines": [
                "ROZPAL DZIŚ WE WNĘTRZU MYM",
                "TWĄ NOW Ą GORLIWOŚĆ I MOC",
                "ROZPAL DZIŚ, O PANIE MÓJ",
                "TWÓJ NOWY OGIEŃ I MOC"
            ]
        },
        {
            "lines": [
                "OGIEŃ TWÓJ OCZYSZCZA MNIE",
                "DODAJE SIŁY, UZDRAWIA MNIE",
                "ROZPAL GO W GŁĘBI DUSZY MEJ"
            ]
        }
    ]
}