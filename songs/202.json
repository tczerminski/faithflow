{
    "id": 202,
    "title": "JEZU MIŁY DUSZY MEJ",
    "verses": [
        {
            "lines": [
                "JEZU, MIŁY DUSZY MEJ",
                "JEZU – NIGDY NIE ZOSTAWISZ MNIE",
                "WYRWAŁEŚ MNIE",
                "Z BŁOTNISTEGO DNA",
                "NA SKALE POSTAWIŁEŚ",
                "I TERAZ WIEM"
            ]
        },
        {
            "lines": [
                "JA KOCHAM, POTRZEBUJĘ",
                "CHOĆBY RUNĄŁ ŚWIAT",
                "TY NIE OPUŚCISZ MNIE",
                "MÓJ ZBAWCO, MÓJ PRZYJACIELU",
                "BĘDĘ WIELBIĆ CIĘ",
                "DO KOŃCA MOICH DNI"
            ]
        },
        {
            "lines": [
                "WIELBIĘ CIĘ, PANIE",
                "ZE WSZYSTKICH MOICH SIŁ"
            ]
        }
    ]
}