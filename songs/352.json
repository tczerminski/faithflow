    "verses": [
        {
            "lines": [
                "O TAK, TAK, TAK,",
                "PANIE MÓWIĘ TAK, TWEMU SŁOWU.",
                "O TAK, TAK, TAK,",
                "PANIE MÓWIĘ TAK, TWOJEJ WOLI.",
