{
    "id": 201,
    "title": "JEZU… ŚWIĘTUJEMY DZIŚ",
    "verses": [
        {
            "lines": [
                "JEZU! JEZU!",
                "ŚWIĘTUJEMY DZIŚ ZWYCIĘSTWO TWE",
                "JEZU! JEZU!",
                "TWOJA MIŁOŚĆ MIESZKA W NAS",
                "JEZU! JEZU! WSZYSTKICH",
                "WIĘZIEŃ OTWORZYŁEŚ DRZWI",
                "ŻYCIE PRZYNIOSŁEŚ W DARZE NAM"
            ]
        },
        {
            "lines": [
                "BO DO WOLNOŚCI POWOŁAŁ CHRYSTUS NAS",
                "NIE MA WIĘCEJ JARZMA STRACHU",
                "W CHRYSTUSIE WOLNOŚĆ JEST",
                "W BOŻYM ZWYCIĘSTWIE,",
                "W RADOŚCI ŻYJMY WIĘC",
                "NASZE SERCA ODDAJMY PANU DZIŚ"
            ]
        }
    ]
}