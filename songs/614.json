{
    "id": 614,
    "title": "WIELBIĆ MEGO PANA CHCĘ, BO ON GODZIEN",
    "verses": [
        {
            "lines": [
                "WIELBIĆ MEGO PANA CHCĘ",
                "BO ON GODZIEN CHWAŁY JEST",
                "ON UWALNIA MNIE SAM OD WROGÓW MYCH"
            ]
        },
        {
            "lines": [
                "MÓJ PAN ŻYJE! OPOKĄ MOJĄ ON!",
                "NIECHAJ BĘDZIE WYWYŻSZONY",
                "MÓJ ZBAWICIEL"
            ]
        }
    ]
}