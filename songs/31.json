    "title": "BOŻE MÓJ JESZCZE RAZ",
    "verses": [
        {
            "lines": [
                "BOŻE MÓJ JESZCZE RAZ",
                "OTWIERAM SWE SERCE",
                "BO WIEM – JESTEŚ TU",
                "MODLITW MYCH SŁUCHASZ WCIĄŻ",
                "BEZ WZGLĘDU NA STAN",
                "W JAKIM ZNAJDUJĘ SIĘ"
            ]
        },
        {
            "lines": [
                "JESTEŚ WIERNY, ODPOWIADASZ",
                "SWYM SŁOWEM PRAWDZIWYM",
                "NADZIEJĄ CO TRWA",
                "DOTYKAJĄC MNIE PANIE",
                "UWALNIASZ OD GRZECHÓW",
                "CO WIĄZAŁY MNIE"
            ]
        },
        {
            "lines": [
                "BO DLA CIEBIE PRAGNĘ ŻYĆ"
            ]
        },
        {
            "lines": [
                "DO CIEBIE MÓJ DUCH WOŁA DZIŚ –",
                "WCIĄŻ CIEBIE PRAGNĘ",
                "DUCH WOŁA DZIŚ – WCIĄŻ CIEBIE SZUKA",
                "MÓJ DUCH WOŁA DZIŚ – JESTEM TAK WDZIĘCZNY",
                "DUCH WOŁA DZIŚ – JESTEŚ WSPANIAŁY TAK"
            ]
        }
    ]
