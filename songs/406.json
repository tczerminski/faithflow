{
    "id": 406,
    "title": "PANIE KIEDY CIEBIE POTRZEBUJĘ",
    "verses": [
        {
            "lines": [
                "PANIE KIEDY CIEBIE POTRZEBUJĘ",
                "TY ZAWSZE PRAGNIESZ POMÓC MI",
                "GDY NADCHODZĄ",
                "CHWILE DOŚWIADCZENIA",
                "OPARCIEM MOIM JESTEŚ TY"
            ]
        },
        {
            "lines": [
                "JESTEŚ ŹRÓDŁEM",
                "MOJEGO UZDROWIENIA",
                "POCIESZASZ KIEDY CZUJĘ ŻAL",
                "JESTEŚ WSPARCIEM",
                "W CHWILI MEJ SŁABOŚCI",
                "POMOCĄ MOJĄ W KAŻDY CZAS"
            ]
        },
        {
            "lines": [
                "WIECZNĄ SKAŁĄ",
                "JEST WSZECHMOCNY BÓG",
                "ON MA MOC",
                "BY UCZYNIĆ CO OBIECAŁ",
                "ZAWSZE WIERNIE",
                "DOTRZYMUJE SWYCH SŁÓW",
                "I DLATEGO ZAWSZE UFAM MU"
            ]
        }
    ]
}