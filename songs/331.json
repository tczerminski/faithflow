{
    "id": 331,
    "title": "NIECH TWA CHWAŁA",
    "verses": [
        {
            "lines": [
                "NIECH TWA CHWAŁA DZIŚ ZSTĄPI TU",
                "NIECH DOSIĘGNIE SWĄ MOCĄ NARODY",
                "NIECH TWA WONNOŚĆ OTOCZY NAS",
                "GDY ZEBRANI SZUKAMY CIĘ"
            ]
        },
        {
            "lines": [
                "STWORZENIA OJCZE",
                "SPEŁNIJ SWÓJ ODWIECZNY PLAN",
                "ZBUDŹ DZIŚ WYBRANYCH SWYCH ARMIĘ",
                "NIECH KROCZY PRZEZ NASZ KRAJ"
            ]
        },
        {
            "lines": [
                "CAŁE STWORZENIE DZIŚ PRAGNIE",
                "BYŚ JUŻ OKAZAŁ SWĄ MOC",
                "WIĘC ZEŚLIJ SWE NAMASZCZENIE",
                "NIECH TERAZ BĘDZIE TEN CZAS"
            ]
        },
        {
            "lines": [
                "NARODÓW WŁADCO",
                "ŚWIAT JESZCZE ZOBACZYĆ MA",
                "TWEJ OBIETNICY SPEŁNIENIE",
                "ZWYCIĘSKI KOŚCIÓŁ TWÓJ",
                "ZWRÓĆ SIĘ KU NAM I NAS DOTKNIJ",
                "W SWEJ SILE UMOCNIJ NAS",
                "POKONAJ DZIŚ NASZĄ SŁABOŚĆ",
                "DO WALKI STANĄĆ JUŻ CZAS"
            ]
        }
    ]
}