{
    "id": 540,
    "title": "TO PRZYKAZANIE",
    "verses": [
        {
            "lines": [
                "TO PRZYKAZANIE JA DZIŚ DAJĘ WAM",
                "BYŚCIE SIĘ MIŁOWALI",
                "JAK JA MIŁUJĘ WAS"
            ]
        },
        {
            "lines": [
                "A WTEDY WSZYSCY POZNAJĄ",
                "ŻEŚCIE MOI",
                "GDY MIŁOŚĆ WZAJEMNĄ",
                "MIEĆ BĘDZIECIE"
            ]
        }
    ]
}