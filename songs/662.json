    "verses": [
        {
            "lines": [
                "ZAŚPIEWAJ BOGU PIEŚNI CHWAŁY",
                "ALLELUJA!"
            ]
        },
        {
            "lines": [
                "GDYŻ PAN KRÓLEM JEST",
                "CAŁEJ ZIEMI",
                "WIĘC ZAWSZE ŚPIEWAJ MU",
                "Z CAŁEGO SERCA",
                "O KLASZCZCIE, KRZYCZCIE WRAZ!",
                "WSZYSCY LUDZIE",
                "BO ON JEST GODZIEN",
                "WSZELKIEJ CZCI!"
            ]
        }
    ]
}