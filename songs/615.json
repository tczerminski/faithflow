{
    "id": 615,
    "title": "WIELBIĆ MEGO PANA CHCĘ, ON ZMIENIŁ",
    "verses": [
        {
            "lines": [
                "WIELBIĆ MEGO PANA CHCĘ"
            ]
        },
        {
            "lines": [
                "ON ZMIENIŁ ŻYCIE ME",
                "PEWNEGO DNIA",
                "ON ZMIENIŁ SERCE ME",
                "I WSKAZAŁ MI DROGĘ SWĄ",
                "DAŁ ŻYCIE WIECZNE MI",
                "WIĘC CHCĘ POWIEDZIEĆ, ŻE",
                "WIELBIĆ MEGO PANA CHCĘ!"
            ]
        }
    ]
}