{
    "id": 626,
    "title": "WIEM, ŻE ON ODKUPIŁ MNIE",
    "verses": [
        {
            "lines": [
                "WIEM, ŻE ON ODKUPIŁ MNIE",
                "JEGO KREW ZMYŁA MÓJ GRZECH",
                "WIERZĘ W TO, WIERZĘ W TO.",
                "MĄ HAŃBĘ NA SIEBIE WZIĄŁ",
                "MÓJ BÓL UZDROWIONY W NIM JEST",
                "WIERZĘ W TO, WIERZĘ W TO."
            ]
        },
        {
            "lines": [
                "PODNIOSĘ SZTANDAR,",
                "BO MÓJ PAN POKONAŁ ŚMIERĆ"
            ]
        },
        {
            "lines": [
                "ŻYJE ZBAWCA NASZ",
                "ON POKONAŁ ŚMIERĆ"
            ]
        },
        {
            "lines": [
                "POWSTANĘ Z TOBĄ",
                "UWALNIASZ MNIE",
                "ZATAŃCZĘ DZIŚ NA SZCZYTACH GÓR",
                "KRÓLESTWO WIDZĄC TWE"
            ]
        }
    ]
}