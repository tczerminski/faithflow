    "title": "ON MOŻE",
    "verses": [
        {
            "lines": [
                "ON MOŻE! ON MOŻE",
                "JA WIEM, ŻE ON MOŻE",
                "JA WIEM, ŻE JEZUS MOŻE",
                "DOPOMÓC MI"
            ]
        },
        {
            "lines": [
                "ON LECZY CHORE SERCA",
                "UTWIERDZA CHROMYCH KROK",
                "ON KARMI GŁODNYCH, LECZY TRĄD",
                "PRZYWRACA ŚLEPYM WZROK!"
            ]
        }
    ]
}