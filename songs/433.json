{
    "id": 433,
    "title": "POWIETRZEM MYM",
    "verses": [
        {
            "lines": [
                "POWIETRZEM MYM",
                "POKARMEM JESTEŚ TY",
                "CIEBIE PRAGNĘ WCIĄŻ",
                "TYŚ SKARBEM MYM",
                "UCIECZKĄ JESTEŚ TY",
                "CIEBIE PRAGNĘ WCIĄŻ"
            ]
        },
        {
            "lines": [
                "CHCĘ TYLKO CIEBIE UWIELBIAĆ",
                "W TWYM DUCHU W PRAWDZIE TWEJ",
                "PROSZĘ NIECH NAMASZCZENIE",
                "JAK CIEPŁY DESZCZ NA MNIE",
                "SPŁYNIE Z NIEBA"
            ]
        },
        {
            "lines": [
                "DUCHU ŚWIĘTY, DUCHU ŚWIĘTY",
                "PRZYJDŹ DUCHU ŚWIĘTY",
                "PRZYJDŹ BY WYLAĆ NAMASZCZENIE"
            ]
        },
        {
            "lines": [
                "DAJ NAM SWĄ SIŁĘ",
                "DAJ NAM SWĄ MOC",
                "PROSIMY PANIE",
                "OTWÓRZ NIEBO, OTWÓRZ NIEBO"
            ]
        }
    ]
}