{
    "id": 473,
    "title": "SKACZ W GÓRĘ",
    "verses": [
        {
            "lines": [
                "SKACZ W GÓRĘ PANA CHWAL",
                "WYWYŻSZAJ IMIĘ JEZUS",
                "UWIELBIAJ GO",
                "SKACZ W GÓRĘ PANA CHWAL",
                "ON SKAŁĄ TWEGO ŻYCIA JEST"
            ]
        },
        {
            "lines": [
                "GDY DAWID KIEDYŚ KRÓLEM BYŁ",
                "RADOWAŁ SIĘ PRZED BOGIEM",
                "ZE WSZYSTKICH SIŁ",
                "CHOĆ CÓRKA SAULA WZGARDZIŁA NIM",
                "ON WCALE SIĘ NIE PRZEJĄŁ TYM"
            ]
        },
        {
            "lines": [
                "GDY DZISIAJ ŚWIAT TEN WYŚMIEJE CIĘ",
                "TY TRWAJ MOCNO W BOGU",
                "PAMIĘTAJ, ŻE ON ŻYJE W TOBIE",
                "I CHOĆ TRUDNE DNI",
                "ZGINĄĆ NIE POZWOLI CI"
            ]
        }
    ]
}