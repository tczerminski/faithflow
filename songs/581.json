{
    "id": 581,
    "title": "UCZYŃ PANIE ŻYCIE ME",
    "verses": [
        {
            "lines": [
                "UCZYŃ PANIE ŻYCIE ME",
                "OFIARĄ MIŁĄ CI",
                "MNIE CAŁEGO UŻYĆ CHCIEJ",
                "BY WIELBIĆ IMIĘ TWE"
            ]
        },
        {
            "lines": [
                "WEŹ MÓJ GŁOS I DAJ MI PIEŚŃ",
                "PIEŚŃ DLA KRÓLA CHWAŁ",
                "PRAGNĘ CAŁKOWICIE BYĆ",
                "WONNOŚCIĄ MIŁĄ CI!"
            ]
        },
        {
            "lines": [
                "OTWIERAM PANIE SERCE ME",
                "WYZNAJĄC WINĘ SWĄ",
                "ZŁAMANY I SKRUSZONY DUCH",
                "NIECH WIELBI IMIĘ TWE"
            ]
        },
        {
            "lines": [
                "OTWÓRZ PANIE USTA ME",
                "BYM ZAWSZE WIELBIŁ CIĘ",
                "WIĘC PRZYJMIJ DZIŚ OFIARĘ MĄ",
                "TĘ WONNOŚĆ MIŁĄ CI"
            ]
        }
    ]
}