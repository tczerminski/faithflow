{
    "id": 554,
    "title": "TY DAŁEŚ MI ŻYCIE",
    "verses": [
        {
            "lines": [
                "TY DAŁEŚ MI ŻYCIE I UŚMIECH NA TWARZ",
                "TY DAŁEŚ JEZUSA, BO TAK KOCHASZ NAS",
                "DZIĘKUJE OJCZE STWORZYŁEŚ MNIE",
                "NA OBRAZ SWÓJ, NA OBRAZ SWÓJ"
            ]
        },
        {
            "lines": [
                "O PANIE GDYBYM MOTYLEM BYŁ",
                "ZA SKRZYDŁA PIĘKNE DZIĘKOWAŁBYM",
                "A GDYBY PTASZKA SPOTKAŁ MNIE LOS",
                "DZIĘKOWAŁBYM ZA SWÓJ PIĘ KNY GŁOS"
            ]
        },
        {
            "lines": [
                "A GDYBYM NAWET SŁONIEM BYŁ",
                "DZIĘKOWAĆ BYM CHCIAŁ",
                "ZA SWĄ TRĄBĘ I KŁY",
                "DZIĘKUJE OJCZE STWORZYŁEŚ MNIE",
                "NA OBRAZ SWÓJ, NA OBRAZ SWÓJ"
            ]
        }
    ]
}