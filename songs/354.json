{
    "id": 354,
    "title": "O WSTAŃ, BŁOGOSŁAW BOGU DZIŚ",
    "verses": [
        {
            "lines": [
                "O WSTAŃ! BŁOGOSŁAW BOGU DZIŚ",
                "TERAZ I ZAWSZE, WCIĄŻ",
                "AŻ NA WIEKI"
            ]
        },
        {
            "lines": [
                "NIECH IMIĘ TWE O PANIE",
                "ROZSŁAWIA ŚWIAT",
                "PRZEWYŻSZASZ WSZYSTKO",
                "CZYM CHWAŁĘ MOGĘ CI DAĆ",
                "NIECH IMIĘ TWE O PANIE",
                "ROZSŁAWIA ŚWIAT",
                "BO WYWYŻSZONY JESTEŚ NA WIEKI"
            ]
        }
    ]
}