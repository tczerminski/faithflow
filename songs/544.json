{
    "id": 544,
    "title": "TOUCH THE SKY",
    "verses": [
        {
            "lines": [
                "CO KRYJE SIĘ NAD NIEBEM GWIAZD",
                "GDZIE NIEZDOBYTYCH SZCZYTÓW BLASK",
                "CHOĆ PRZYSZŁO MI Z WYSOKA SPAŚĆ",
                "NA DOLE NIEBO ZNALAZŁO MNIE"
            ]
        },
        {
            "lines": [
                "SERCA BICIE, MOJE ŻYCIE",
                "ZNAJDUJĘ, GDY WSZYSTKO DAJE CI",
                "ZNOWU SPADAM NA KOLANA",
                "DOSIĘGAM CIĘ, KIEDY UNIŻAM SIĘ"
            ]
        },
        {
            "lines": [
                "CO KRYJĄ BLIZNY TWOICH RANNY",
                "WOLNOŚCI TWEJ BEZCENNY DAR",
                "GDY MOJE SERCE KUPIŁ ŚWIAT",
                "ODDAŁEŚ NIEBO, BY WYKUPIĆ MNIE"
            ]
        },
        {
            "lines": [
                "JESTEM TU, ZNÓW U TWOICH STÓP",
                "CAŁYM SERCEM JUŻ",
                "JA NALEŻĘ DO CIEBIE",
                "MIŁOŚCIĄ SWĄ ZNÓW OGARNIJ MNIE",
                "MOJA DUSZA CHCE",
                "TAŃCZYĆ DO RYTMU NIEBA"
            ]
        }
    ]
}