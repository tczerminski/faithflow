{
    "id": 603,
    "title": "W TWE RĘCE DZIŚ",
    "verses": [
        {
            "lines": [
                "W TWE RĘCE DZIŚ",
                "ZNÓW ODDAJĘ SIĘ",
                "ME ŻYCIE CHCĘ DAĆ TOBIE",
                "TY W DŁONIACH SWYCH",
                "TRZYMASZ CAŁY MÓJ ŚWIAT",
                "WIĘC JESTEM TWÓJ NA ZAWSZE"
            ]
        },
        {
            "lines": [
                "WIERZĘ W CIEBIE JEZU MÓJ",
                "TWOIM JESTEM JEZU MÓJ",
                "DLA CIEBIE ŻYJĘ DZIŚ",
                "DLA CIEBIE ŚPIEWAM PIEŚŃ",
                "BO JESTEM TWÓJ"
            ]
        },
        {
            "lines": [
                "CHCĘ Z TOBĄ IŚĆ GDZIEKOLWIEK BYŚ SZEDŁ",
                "PRZEZ RADOŚĆ I ŁZY BO UFAM CI",
                "NA DROGACH TWYCH",
                "CHCĘ ŻYĆ TRZYMAJĄC SIĘ",
                "OBIETNIC TWYCH NA ZAWSZE"
            ]
        },
        {
            "lines": [
                "(: BĘDĘ WIELBIĆ :)",
                "BĘDĘ WIELBIĆ CIĘ"
            ]
        }
    ]
}