{
    "id": 651,
    "title": "WYZNAJEMY NASZYMI USTAMI",
    "verses": [
        {
            "lines": [
                "WYZNAJEMY NASZYMI USTAMI",
                "WIARA W SERCACH",
                "POTWIERDZA TEN CUD",
                "PAN WZIĄŁ NASZE PRZESTĘPSTWA",
                "NA SIEBIE",
                "ABY WYKUPIĆ SWÓJ LUD",
                "KUPIĆ SWÓJ LUD"
            ]
        },
        {
            "lines": [
                "NASZYM OJCEM JEST BÓG",
                "MY – JEGO LUD",
                "ON ZMYŁ NASZ GRZECH",
                "I PRZEBACZYŁ NAM DŁUG",
                "ŚWIĘTY DAŁ STRÓJ",
                "DAR Z ŁASKI SWEJ",
                "SPRAWIŁA TO ŚWIĘTA KREW",
                "CHRYSTUSA KREW"
            ]
        },
        {
            "lines": [
                "NIECH SŁUCHAJĄ NAS",
                "DUCHY CIEMNOŚCI",
                "NIECH ZOBACZĄ",
                "GDZIE STAWIA NAS BÓG",
                "MY JESTEŚMY NARODEM WYBRANYM",
                "JEZUS WYKUPIŁ NAS KRWIĄ",
                "SWĄ WŁASNĄ KRWIĄ"
            ]
        }
    ]
}