{
    "id": 136,
    "title": "GODZIEN CHWAŁY",
    "verses": [
        {
            "lines": [
                "GODZIEN CHWAŁY JEST BARANEK",
                "KTÓRY POSZEDŁ NA ŚMIERĆ",
                "TEMU, KTÓRY SIEDZI NA TRONIE",
                "ŚPIEWAJ NOWĄ PIEŚŃ",
                "ŚWIĘTY, ŚWIĘTY JEST BARANEK",
                "KTÓRY POSZEDŁ NA ŚMIERĆ",
                "NOWĄ PIEŚNIĄ UWIELBIĘ ZBAWCĘ",
                "ON KRÓLEM ŁASKI JEST",
                "ŚWIĘTY, ŚWIĘTY, ŚWIĘTY",
                "JEST PAN NASZ BÓG WSZECHMOCNY",
                "ON BYŁ I JEST I PRZYJDZIE ZNÓW",
                "Z CAŁYM STWORZENIEM ŚPIEWAM",
                "KRÓL OWI KRÓLÓW PIEŚŃ",
                "TYŚ WSZYSTKIM DLA MNIE JEST",
                "I BĘDĘ CIĘ WIELBIĆ!"
            ]
        },
        {
            "lines": [
                "ODZIANY TĘCZĄ PIĘKNYCH KOLORÓW",
                "W BŁYSKAWIC BLASK I GRZMOTY BURZY",
                "TOBIE JEDYNY KRÓLU",
                "NIECH PŁYNIE WDZIĘCZNOŚĆ, MOC",
                "BŁOGOSŁAWIEŃSTWO I CHWAŁA"
            ]
        },
        {
            "lines": [
                "KIEDY SŁYSZĘ TWOJE IMIĘ",
                "DUCH MÓJ PRAGNIE WIELBIĆ CIĘ",
                "TO IMIĘ WODĄ ŻYCIA",
                "ODDECHEM MYM I MOCĄ",
                "CUDOWNĄ TAJEMNICĄ JEST"
            ]
        }
    ]
}