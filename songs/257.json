{
    "id": 257,
    "title": "KTÓRZY OCZEKUJĄ PANA",
    "verses": [
        {
            "lines": [
                "KTÓRZY OCZEKUJĄ PANA",
                "NABYWAJĄ NOWYCH SIŁ",
                "PODNOSZĄ SIĘ W GÓRĘ JAK ORŁY",
                "CHODZĄ, A NIE USTAWAJĄ",
                "BIEGNĄ, NIE SPRACUJĄ SIĘ",
                "NAUCZ MNIE OCZEKIWAĆ CIĘ"
            ]
        },
        {
            "lines": [
                "NAUCZ PANIE CZEKAĆ CIĘ",
                "NA KOLANACH AŻ DO DNIA",
                "AŻ NA MOJĄ MODLITWĘ",
                "ODPOWIEDŹ SWĄ DASZ",
                "NAUCZ NIE SZUKAĆ TEGO",
                "CO CZŁOWIEK DA MI",
                "TYLKO CZEKAĆ NA CIEBIE",
                "NA TO, CO DASZ TY"
            ]
        }
    ]
}