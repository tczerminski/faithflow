{
    "id": 190,
    "title": "JESTEŚ ŚWIĘTY, WSZECHMOGĄCY",
    "verses": [
        {
            "lines": [
                "JESTEŚ ŚWIĘTY, WSZECHMOGĄCY,",
                "GODZIEN CHWAŁY, CHWAŁY I CZCI",
                "CHCĘ PODĄŻAĆ WCIĄŻ ZA TOBĄ,",
                "KOCHAĆ CIEBIE DO KOŃCA DNI"
            ]
        },
        {
            "lines": [
                "(: BĘDĘ ŚPIEWAŁ I WIELBIŁ",
                "KRÓLA, KTÓRY JEST GODZIEN",
                "BĘDĘ KOCHAŁ, WYWYŻSZAŁ,",
                "PRZED MYM PANEM SIĘ SKŁONIĘ :)",
                "TYŚ POKOJEM MYM,",
                "TO DLA CIEBIE PRAGNĘ ŻYĆ"
            ]
        },
        {
            "lines": [
                "TYŚ JEST PANÓW PAN,",
                "TYŚ JEST KRÓLÓW KRÓL",
                "WSZECHMOGĄCY BÓG,",
                "WSZYSTKIM RZĄDZISZ TY",
                "TYŚ EMANUEL, WIELKIE IMIĘ TWE",
                "TYŚ POKOJU PAN,",
                "TYŚ BARANKIEM JEST"
            ]
        },
        {
            "lines": [
                "TYŚ JEST ŻYWY BÓG",
                "I ŁASKAWY PAN",
                "TWE KRÓLESTWO TRWA,",
                "TYŚ ODWIECZNY BÓG"
            ]
        },
        {
            "lines": [
                "TYŚ JEST ALFA, OMEGA,",
                "POCZĄTEK I KRES",
                "ODKUPICIEL, PRZYJACIEL,",
                "MESJASZ, CO ZBAWIŁ ŚWIAT",
                "TYŚ POKOJEM MYM,",
                "TO DLA CIEBIE PRAGNĘ ŻYĆ"
            ]
        }
    ]
}