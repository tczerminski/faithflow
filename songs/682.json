{
    "id": 682,
    "title": "ŻYCIEM MYM",
    "verses": [
        {
            "lines": [
                "ŻYCIEM MYM, PRAWDĄ MĄ",
                "JEST MÓJ PAN, MÓJ JEZUS",
                "UMIŁOWAŁEM GO BARDZIEJ",
                "NIŻ CAŁY ŚWIAT"
            ]
        },
        {
            "lines": [
                "PODAŁ MI RĘKĘ SWĄ",
                "I PODNIÓSŁ MNIE Z MYCH GRZECHÓW",
                "OBMYŁ MNIE WŁASNĄ KRWIĄ",
                "I DAŁ NAJBIELSZĄ Z SZAT"
            ]
        },
        {
            "lines": [
                "ZRODZIŁ MNIE PRZEZ MIŁOŚĆ SWĄ",
                "OŻYWIŁ DUCHEM SWYM",
                "ULECZYŁ SERCE ME",
                "Z LICZNYCH MOICH RAN"
            ]
        },
        {
            "lines": [
                "DZIŚ WIEDZIE MNIE",
                "TAM GDZIE WIECZNY DOM",
                "OSŁANIA PŁASZCZEM SWYM",
                "TO WSZYSTKO SPRAWIA ON",
                "JEZUS, MÓJ PAN"
            ]
        }
    ]
}