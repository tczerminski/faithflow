{
    "id": 577,
    "title": "TYŚ SIŁĄ MĄ",
    "verses": [
        {
            "lines": [
                "TYŚ SIŁĄ MĄ, GDY SŁABYM JA",
                "W TOBIE NADZIEJA MOJA TRWA",
                "TY JESTEŚ DLA MNIE WSZYSTKIM",
                "SZUKAM CIĘ PANIE NICZYM SKARB",
                "NIE ZREZYGNUJĘ, JESTEŚ WART",
                "MOJEJ WIERNOŚCI PANIE"
            ]
        },
        {
            "lines": [
                "JEZU, KRÓLEM TYŚ, ŚWIĘTE IMIĘ TWE",
                "JEZU, BARDZO CHCĘ NAŚLADOWAĆ CIĘ"
            ]
        },
        {
            "lines": [
                "KIEDY UPADAM STAJESZ TY",
                "ABY MNIE PODNIEŚĆ, POMÓC IŚĆ",
                "TY JESTEŚ DLA MNIE WSZYSTKIM",
                "KIEDY SPRAGNIONY JESTEM TY",
                "PRZYCHODZISZ ABY DAĆ MI PIĆ",
                "ZE ŹRÓDŁA WODY ŻYWEJ"
            ]
        }
    ]
}