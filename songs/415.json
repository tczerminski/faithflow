{
    "id": 415,
    "title": "PANIE, TY WSZYSTKO MOŻESZ",
    "verses": [
        {
            "lines": [
                "PANIE TY WSZYSTKO MOŻESZ",
                "I WSZYSTKO JEST DLA CIEBIE MOŻLIWE",
                "NIE MA TAKIEJ RZECZY",
                "KTÓREJ TY BYŚ NIE MÓGŁ UCZYNIĆ"
            ]
        },
        {
            "lines": [
                "DO CIEBIE DZIŚ ZWRACAMY SIĘ",
                "TY USŁYSZ NASZE WOŁANIE",
                "ODPOWIEDZ NA MODLITWĘ TĘ",
                "I DAJ NAM POZNAĆ WOLĘ SWĄ"
            ]
        }
    ]
}