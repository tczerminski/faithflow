{
    "id": 250,
    "title": "KORZYMY SIĘ",
    "verses": [
        {
            "lines": [
                "KORZYMY SIĘ PRZED TWYM MAJESTATEM",
                "W DUCHU I W PRAWDZIE SKŁANIAMY SIĘ",
                "TY O BOŻE SAM PRZEBYWASZ",
                "POŚRÓD CHWAŁY SWOJEGO LUDU",
                "WIELBIMY CIĘ Z BOJAŹNIĄ I CZCIĄ"
            ]
        },
        {
            "lines": [
                "NASTAŁ JUŻ PAŃSKI DZIEŃ ODNOWIENIA",
                "SYJON ROZJAŚNIA PRAWDA TWA",
                "LUD TAM UWIELBIA SWOJEGO BOGA",
                "ŚPIEWAJĄC GRAJĄC ILE SIŁ KTO MA"
            ]
        }
    ]
}