{
    "id": 677,
    "title": "ZMIŁUJ SIĘ, PANIE",
    "verses": [
        {
            "lines": [
                "ZMIŁUJ SIĘ PANIE",
                "PROSZĘ CIĘ, ZMIŁUJ NADE MNĄ",
                "PRZEBACZ MÓJ GRZECH",
                "TO, CO ZROBIŁEM BYŁO ZŁE",
                "ZBAW MNIE OD ZŁEGO",
                "NIE ODRZUCAJ MNIE"
            ]
        },
        {
            "lines": [
                "TY KOCHASZ PRAWDĘ",
                "NA DUSZY DNIE SCHOWANĄ",
                "SKRUSZONYM SERCEM",
                "NIE POGARDZISZ PANIE",
                "GDY MNIE OCZYŚCISZ",
                "NAD ŚNIEG BIELSZY SIĘ STANĘ",
                "OTWÓRZ ME USTA",
                "NIECH GŁOSZĄ TWOJĄ CHWAŁĘ"
            ]
        },
        {
            "lines": [
                "SERCE NIEWINNE WE MNIE STWÓRZ",
                "NIECH NIE OPUSZCZA MNIE TWÓJ ŚWIĘTY DUCH",
                "DŹWIĘKIEM RADOŚCI OBUDŹ MNIE",
                "SKRUSZONE KOŚCI ZNÓW PODNIOSĄ SIĘ"
            ]
        },
        {
            "lines": [
                "WINY NASZE PRZEBACZ",
                "WYBAW NAS OD ZŁEGO"
            ]
        }
    ]
}