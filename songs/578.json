    "id": 578,
    "title": "U PANA DZIŚ",
    "verses": [
        {
            "lines": [
                "U PANA DZIŚ",
                "ZOSTAWIAM TROSKI SWE,",
                "ODDAJĘ MU,",
                "ON CHĘTNIE CIĘŻAR MÓJ CHCE NIEŚĆ,",
                "SKŁADAM GO U JEGO STÓP",
                "I ULGĘ CZUJĘ ZNÓW.",
                "MOJE ZWĄTPIENIA, STRACH",
                "U PANA SKŁADAM DZIŚ"
            ]
        }
    ]
}