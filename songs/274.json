                "MIŁOSIERNY, DOBRY",
                "ŚWIĘTY BÓG",
                "TO TY, TO TY",
                "DAJESZ MI POKÓJ I PRZEBACZENIE",
                "W RAMIONA SWE BIERZESZ MNIE",
                "KIEDY UPADAM",
                "TY MNIE PODNOSISZ",
                "W RAMIONACH SWYCH",
                "TRZYMASZ MNIE"
            ]
        }
