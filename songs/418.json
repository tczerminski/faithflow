    "title": "PANIE, WYBACZ MI",
    "verses": [
        {
            "lines": [
                "(: PANIE WYBACZ MI :)",
                "NIEPOSŁUSZEŃSTWO MOJEJ DROGI",
                "(: PANIE WYBACZ MI :)",
                "ŚLEPE OCZY, USZY, KTÓRE NIE CHCĄ SŁYSZEĆ"
            ]
        },
        {
            "lines": [
                "PANIE WYBACZ MI, PANIE WYBACZ MI",
                "MYŚLI SKAŻONE ŚWIATA FILOZOFIĄ",
                "PANIE WYBACZ MI, WYBACZ MI,",
                "PUSTE DNI BEZ CIEBIE"
            ]
        },
        {
            "lines": [
                "LECZ TY WIESZ, ŻE W GŁĘBI SERCA",
                "ZA TOBĄ TĘSKNIŁEM",
                "TAK MOCNO, MOCNO, MOCNO",
                "LECZ TY WIESZ, ŻE W GŁĘBI SERCA",
                "ZA TOBĄ TĘSKNIŁEM",
                "TAK BARDZO"
            ]
        },
        {
            "lines": [
                "(: PANIE WYBACZ MI :)",
                "ŻE ZASIADAŁEM W GRONIE SZYDERCÓW",
                "(: PANIE WYBACZ MI :)",
                "ŻE MIŁOWAŁEM ŚWIAT BARDZIEJ NIŻ CIEBIE"
            ]
        },
        {
            "lines": [
                "PANIE WYBACZ MI, PANIE WYBACZ MI,",
                "ŻE NIE SZUKAŁEM CIEBIE W UKRYCIU",
