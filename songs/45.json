{
    "id": 45,
    "title": "BYŁ ZRANIONY ZA ME WINY",
    "verses": [
        {
            "lines": [
                "BYŁ ZRANIONY ZA ME WINY",
                "STARTY BYŁ ON ZA PRZESTĘPSTWA ME"
            ]
        },
        {
            "lines": [
                "BOLEŚCI MĄŻ WZGARDZONY TAK",
                "CHOĆ BOŻYM SYNEM BYŁ",
                "DO NIEGO DZIŚ WOŁAMY WIĘC",
                "UZDRÓW NAS"
            ]
        },
        {
            "lines": [
                "UZDRÓW NAS, UZDRÓW NAS",
                "MOCĄ SWOICH ŚWIĘTYCH RAN",
                "UZDRÓW NAS",
                "TYŚ PRZEKLEŃSTWO ŚMIERCI",
                "I PIEKŁA MOC",
                "W SWYM KRZYŻU DAWNO ZGNIÓTŁ",
                "WIĘC MOCĄ RAN, ŚWIĘTYCH RAN,",
                "MOCĄ RAN – UZDRÓW NAS"
            ]
        }
    ]
}