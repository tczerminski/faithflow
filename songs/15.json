    "verses": [
        {
            "lines": [
                "BĘDĘ BŁOGOSŁAWIĆ PANU",
                "AŻ DO KOŃCA DNI",
                "CHWAŁA JEMU NIECH CIĄGLE",
                "WYPŁYWA Z MYCH UST"
