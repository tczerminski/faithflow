{
    "id": 515,
    "title": "ŚWIĘTY BÓG, CUDOWNY PAN",
    "verses": [
        {
            "lines": [
                "ŚWIĘTY BÓG, CUDOWNY PAN",
                "ŚWIĘTY DUCH –",
                "WIEJĄCY WCIĄŻ WIATR",
                "WIEJĄCY Z NIEBA"
            ]
        },
        {
            "lines": [
                "PRAGNIEMY ODTĄD JUŻ IŚĆ",
                "GDZIE BOŻA CHWAŁA LŚNI",
                "OBUDZIĆ CAŁY KRAJ TO NASZ CEL",
                "JAK WIELKA ARMIA PANA",
                "TYSIĄCE WALCZY SERC"
            ]
        },
        {
            "lines": [
                "ŚWIĘTĄ KREW WZYWAMY DZIŚ",
                "OGNIU ZSTĄP,",
                "O WIETRZE DZIŚ WIEJ",
                "WIEJ DZISIAJ Z NIEBA"
            ]
        }
    ]
}