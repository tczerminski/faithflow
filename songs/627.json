{
    "id": 627,
    "title": "WIERZĘ W BOGA, OJCA, STWÓRCĘ",
    "verses": [
        {
            "lines": [
                "WIERZĘ W BOGA OJCA STWÓRCĘ",
                "I W JEZUSA JEGO SYNA",
                "ON NA KRZYŻU ZA MNIE UMARŁ",
                "ŻYJE DZIŚ!"
            ]
        },
        {
            "lines": [
                "JEZUS (JEZUS) – MOIM KRÓLEM",
                "JEZUS (JEZUS) – MOIM PANEM",
                "JEZUS (JEZUS) – MOIM ŻYCIEM",
                "ZBAWIENIEM MOIM JEST"
            ]
        },
        {
            "lines": [
                "JEZUS – ON KRÓLEM JEST",
                "JEZUS – ON PANEM JEST",
                "JEZUS – ON ŻYCIEM JEST",
                "ZBAWIŁ MNIE"
            ]
        },
        {
            "lines": [
                "TAK, JEZU MÓJ UMARŁEŚ ZA MÓJ GRZECH",
                "TAK, BOŻE MÓJ TY ŻYJESZ WE MNIE",
                "O ZBAWCO MÓJ ZBAWIŁEŚ MNIE",
                "ŻYJESZ DZIŚ ABYM JA MÓGŁ ŻYĆ"
            ]
        }
    ]
}