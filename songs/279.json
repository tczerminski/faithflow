    "title": "MOC, CZEŚĆ I CHWAŁĘ",
    "verses": [
        {
            "lines": [
                "MOC, CZEŚĆ I CHWAŁĘ,",
                "BŁOGOSŁAWIEŃSTWO",
                "NIECHAJ PRZYJMIE ODWIECZNY PAN",
                "NIECH KAŻDY NARÓD, CAŁE STWORZENIE",
                "ODWIECZNEMU KŁANIAJĄ SIĘ"
            ]
        },
        {
            "lines": [
                "KAŻDY JĘZYK W NIEBIE, NA ZIEMI",
                "OGŁOSI CHWAŁĘ TWĄ",
                "KOLANA PRZED TRONEM TWYM",
                "SIĘ UGNĄ",
                "WYWYŻSZONY BĘDZIESZ, O BOŻE",
                "A KRÓLESTWO TWOJE BĘDZIE JUŻ NA WIEKI TRWAĆ"
            ]
        },
        {
            "lines": [
                "KRÓLESTWO POTĘŻNE OBJAWI SIĘ",
                "ŚPIEWAJ ODWIECZNEMU DZIŚ PIEŚŃ",
                "A JEGO ŚWIĘTOŚCI NIE ZAĆMI NIC",
                "ŚPIEWAJ ODWIECZNEMU DZIŚ PIEŚŃ"
            ]
        }
    ]
