    "id": 359,
    "title": "OCZYŚĆ MNIE OGNIEM",
    "verses": [
        {
            "lines": [
                "OCZYŚĆ MNIE OGNIEM SWEGO DUCHA",
                "OCZYŚĆ MNIE I UCZYŃ BLISKIM CI",
                "POMÓŻ MI ODRZUCIĆ TO, CO MOJE",
                "W ŻYCIU MYM.",
                "UŻYJ MNIE TAK JAK CHCESZ"
            ]
        }
    ]
}