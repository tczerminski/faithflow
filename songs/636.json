{
    "id": 636,
    "title": "WOŁANIE SERCA",
    "verses": [
        {
            "lines": [
                "W KAŻDYM KRYJE SIĘ TĘSKNOTA",
                "SERCE KOGOŚ WOŁA WCIĄŻ",
                "CHOĆBYŚ PRAGNĄŁ NIE UCISZYSZ GO",
                "KAŻDE SERCE SZUKA NIE USTANIE",
                "BOGA, KTÓRY STWORZYŁ JE",
                "W JEGO RĘKACH PRAGNIE ZNALEŹĆ SIĘ"
            ]
        },
        {
            "lines": [
                "WOŁANIE SERCA USTANIE GDY OTWORZYSZ JE",
                "GDY BÓG W NIM ZAMIESZKA I ZECHCESZ BY PROWADZIŁ CIĘ"
            ]
        },
        {
            "lines": [
                "MIŁOŚĆ, ZDROWIE, KARIERA, CHOĆ WAŻNE",
                "NIE ZAPEWNIĄ CI SZCZĘŚCIA TAK",
                "BYŚ JUŻ NIE CZUŁ, ŻE WCIĄŻ CZEGOŚ BRAK",
                "TWOJE SERE STWORZYŁ BÓG W TEN SPOSÓB",
                "BY PEŁNIE CZUŁO TYLKO GDY ON JEST W NIM",
                "CHCESZ JEJ ZAZNAĆ? OTWÓRZ SERCA DRZWI"
            ]
        },
        {
            "lines": [
                "BÓG JEST WIERNY I NA CIEBIE CZEKA",
                "ODPOWIEDZI NA PYTANIA ZNA",
                "GDY POPROSISZ – WSZYSTKO CI WYBACZY",
                "GDY UWIERZYSZ – ŻYCIE WIECZNE DA"
            ]
        }
    ]
}