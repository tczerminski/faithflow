{
    "id": 631,
    "title": "WITAJ JEZU! O CZEŚĆ CI KRÓLU",
    "verses": [
        {
            "lines": [
                "WITAJ JEZU!",
                "O CZEŚĆ CI KRÓLU",
                "PRZYJMIJ ŚPIEW NASZ,",
                "MODLITWĘ, WIELBIENIE",
                "BOŻY SYNU!"
            ]
        },
        {
            "lines": [
                "DZIŚ KU TOBIE",
                "WZNOSIMY RĘCE SWE",
                "NA ZNAK SZACUNKU, MIŁOŚCI,",
                "WDZIĘCZNOŚCI O, ALLELUJA!"
            ]
        },
        {
            "lines": [
                "ALLELUJA"
            ]
        }
    ]
}