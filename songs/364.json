{
    "id": 364,
    "title": "ODDYCHAĆ TOBĄ CHCĘ",
    "verses": [
        {
            "lines": [
                "ODDYCHAĆ TOBĄ CHCĘ",
                "TY ŻYJESZ W DUSZY MEJ",
                "OBECNOŚĆ ŚWIĘTA",
                "WE MNIE – SAM BÓG",
                "TYŚ MÓJ CODZIENNY CHLEB",
                "SPOŻYWAĆ CIEBIE CHCĘ",
                "TWE SŁOWO ŚWIĘTE",
                "KARMI DZIŚ MNIE"
            ]
        },
        {
            "lines": [
                "A JA TAK PRAGNĘ CIEBIE",
                "BO WIEM – BEZ CIEBIE ZGINĘ"
            ]
        }
    ]
}