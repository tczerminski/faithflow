{
    "id": 78,
    "title": "CZAS NAJWYŻSZY, ABY BOŻA CHWAŁA",
    "verses": [
        {
            "lines": [
                "CZAS NAJWYŻSZY,",
                "ABY BOŻA CHWAŁA",
                "GŁOŚNO BRZMIAŁA DZIŚ W ŚWIĄTYNI",
                "JEGO DUCHA"
            ]
        },
        {
            "lines": [
                "WIĘC BĄDŹ GOTOWY JUŻ",
                "UWIELBIENIA PRZYNIEŚĆ PIEŚŃ",
                "BOŻA CHWAŁA",
                "DZIŚ WYPEŁNIA MIEJSCE TO"
            ]
        },
        {
            "lines": [
                "CHWALMY GO,",
                "TAK CHWALMY GO"
            ]
        },
        {
            "lines": [
                "BÓG STALE MIESZKA",
                "W CHWAŁACH LUDU SWEGO",
                "PRAGNIE BYŚ GO WIELBIŁ,",
                "ON ODPOWIE CI"
            ]
        }
    ]
}