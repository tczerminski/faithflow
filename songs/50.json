    "verses": [
        {
            "lines": [
                "CHCĘ PANIE CHWALIĆ CIĘ",
                "WIĘCEJ Z DNIA NA DZIEŃ",
                "CHCĘ PANIE CHWALIĆ CIĘ",
                "WIĘCEJ Z DNIA NA DZIEŃ",
                "SZUKAĆ TWARZY TWEJ",
                "I POZNAWAĆ WOLĘ TWĄ",
                "CHCĘ PANIE CHWALIĆ CIĘ"
            ]
        },
        {
            "lines": [
                "PTAKI NA NIEBIE CIEBIE CHWALIĆ CHCĄ",
