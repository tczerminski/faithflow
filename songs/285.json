{
    "id": 285,
    "title": "MÓJ JEZU, MÓJ ZBAWCO",
    "verses": [
        {
            "lines": [
                "MÓJ JEZU, MÓJ ZBAW CO",
                "NIE MA NIKOGO JAK TY",
                "PO WSZYSTKIE DNI OGŁASZAĆ CHCĘ",
                "TWĄ WIELKĄ MIŁOŚĆ, CUDA TWE"
            ]
        },
        {
            "lines": [
                "POCIESZASZ I CHRONISZ",
                "TYŚ SIŁĄ, UCIECZKĄ JEST",
                "NIECH WSZYSTKO TO, CO ODDECH MA",
                "WIELBI BOGA Z WSZYSTKICH SIŁ"
            ]
        },
        {
            "lines": [
                "DZIŚ CAŁA ZIEMIO",
                "DLA PANA WIĘC KRZYCZ",
                "ON TRWA W MAJESTACIE,",
                "ON ŚWIĘTY JEST",
                "GÓRY SIĘ KŁONIĄ, A MORZE GRZMI",
                "NA DŹWIĘK TWOICH SŁÓW"
            ]
        },
        {
            "lines": [
                "BĘDĘ CIĘ WIELBIĆ,",
                "CHCĘ ŚPIEWAĆ CI PIEŚŃ",
                "NA ZAWSZE CIĘ KOCHAĆ,",
                "PRZY TOBIE BYĆ",
                "TYŚ KRÓLEM KRÓLÓW",
                "TYŚ BOGIEM PRAWDZIWYM JEST"
            ]
        }
    ]
}