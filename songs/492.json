{
    "id": 492,
    "title": "STWÓRZ WE MNIE SERCE CZYSTE",
    "verses": [
        {
            "lines": [
                "STWÓRZ WE MNIE SERCE CZYSTE BOŻE",
                "I DUCHA PRAWEGO PRZYWRÓĆ MI"
            ]
        },
        {
            "lines": [
                "NIE ODRZUCAJ MNIE OD OBECNOŚCI TWOJEJ",
                "NIE ODBIERAJ MI TWEGO DUCHA",
                "PRZYWRÓĆ WE MNIE RADOŚĆ",
                "Z WYBAWIENIA MEGO",
                "I DUCHA PRAWEGO PRZYWRÓĆ MI"
            ]
        }
    ]
}