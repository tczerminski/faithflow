{
    "id": 426,
    "title": "POLSKA OD MORZA DO TATR",
    "verses": [
        {
            "lines": [
                "POLSKA OD MORZA DO TATR,",
                "POLSKA OD ODRY PO BUG,",
                "JAKI PIĘKNY TO KRAJ,",
                "O KTÓRY CIĄGLE MODLĘ SIĘ"
            ]
        },
        {
            "lines": [
                "PRAGNĘ ABY TEN KRAJ",
                "POZNAŁ SWEGO KRÓLA CHWAŁ",
                "DZISIAJ OGŁASZAM WAM,",
                "ŻE JEZUS CHRYSTUS KRÓLEM JEST!"
            ]
        }
    ]
}