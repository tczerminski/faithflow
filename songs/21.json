{
    "id": 21,
    "title": "BŁOGOSŁAWIĘ CIĘ",
    "verses": [
        {
            "lines": [
                "BŁOGOSŁAWIĘ CIĘ",
                "KIEDY ZIEMIA WYDAJE PLON",
                "GDY OBFITOŚĆ PANIE ZLEWASZ SWĄ",
                "BŁOGOSŁAWIĘ CIĘ"
            ]
        },
        {
            "lines": [
                "BŁOGOSŁAWIĘ CIĘ",
                "GDY PUSTYNIA",
                "W KRĄG OTACZA MNIE",
                "CHOĆBYM TEŻ PRZEZ PUSTKOWIE SZEDŁ",
                "BŁOGOSŁAWIĘ CIĘ"
            ]
        },
        {
            "lines": [
                "ZA TWĄ ŁASKĘ, KTÓRĄ ZSYŁASZ",
                "JA CHWALĘ CIĘ",
                "CHOĆBY CIEMNOŚĆ MNIE PRZYKRYŁA",
                "JA POWIEM, ŻE:"
            ]
        },
        {
            "lines": [
                "PANIE BŁOGOSŁAWIĘ IMIĘ TWE",
                "BŁOGOSŁAWIĘ CIĘ",
                "PANIE BŁOGOSŁAWIĘ IMIĘ TWE",
                "BŁOGOSŁAWIĘ ŚWIĘTE IMIĘ TWE"
            ]
        },
        {
            "lines": [
                "BŁOGOSŁAWIĘ CIĘ",
                "KIEDY SŁOŃCA BLASK OŚWIETLA DZIEŃ",
                "CAŁY ŚWIAT DOKOŁA PIĘKNY JEST",
                "BŁOGOSŁAWIĘ CIĘ"
            ]
        },
        {
            "lines": [
                "BŁOGOSŁAWIĘ CIĘ",
                "KIEDY DROGA MA CIERPIENIEM JEST",
                "CHOCIAŻ WIEM, ŻE WOKÓŁ TYLKO BÓL",
                "BŁOGOSŁAWIĘ CIĘ"
            ]
        },
        {
            "lines": [
                "TY DAJESZ Z WOLI SWEJ",
                "ZABIERASZ KIEDY CHCESZ",
                "W MYM SERCU ZAWSZE CHCĘ",
                "BŁOGOSŁAWIĆ CIĘ"
            ]
        }
    ]
}