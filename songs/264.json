{
    "id": 264,
    "title": "LUDZIE CIĄGLE SIĘ ŚCIGAJĄ",
    "verses": [
        {
            "lines": [
                "LUDZIE CIĄGLE SIĘ ŚCIGAJĄ",
                "LEDWO SOBIE RADĘ DAJĄ",
                "BIEGNĄ ŚLEPO TAK PRZED SIEBIE",
                "WYŚCIG SZCZURÓW NIE DLA CIEBIE"
            ]
        },
        {
            "lines": [
                "SZYBCIEJ WYŻEJ CORAZ DALEJ",
                "LECZ UWAŻAJ NIE OSZALEJ",
                "CHCESZ NA MECIE BYĆ Z MEDALEM",
                "ŻYCIEM ODDAJ BOGU CHWAŁĘ"
            ]
        },
        {
            "lines": [
                "PIERWSZY W WYRZUCANIU ŚMIECI",
                "MISTRZ OD MOPA W LIDZE DZIECI",
                "LEPSZY CHCĘ BYĆ W KAŻDYM CALU",
                "CORAZ BLIŻEJ DO MEDALU"
            ]
        },
        {
            "lines": [
                "CHCĘ SIĘ ŚCIGAĆ ZE SOBĄ",
                "CHOĆBYM WSTAŁ LEWĄ NOGĄ",
                "PRAGNĘ LEPSZYM BYĆ",
                "PRAGNĘ LEPSZYM BYĆ CZŁOWIEKIEM",
                "BLIŻEJ CELU Z KAŻDYM MECZEM"
            ]
        }
    ]
}