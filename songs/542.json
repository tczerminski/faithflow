{
    "id": 542,
    "title": "TO TAJEMNICĄ JEST",
    "verses": [
        {
            "lines": [
                "TO TAJEMNICĄ JEST",
                "ŻE CHRYSTUS WYBRAŁ CIEBIE I MNIE",
                "BYM OBJAWIENIEM JEGO CHWAŁY BYŁ",
                "KRÓLEWSKI ŚWIĘTY LUD",
                "WYBRANY ODDZIELONY",
                "DLA SWEGO KRÓLA DZIŚ GOTUJE SIĘ"
            ]
        },
        {
            "lines": [
                "PANIE JEZU PRZYJDŹ,",
                "PANIE JEZU PRZYJDŹ",
                "PANIE PRZYJDŹ, WOŁA CIĘ",
                "TWA OBLUBIENICA"
            ]
        },
        {
            "lines": [
                "SPLENDOREM OTOCZONA",
                "NA GŁOWIE JEJ KORONA",
                "A KRÓL PORUSZONY JEST JEJ PIĘKNEM",
                "ODZIANA W SPRAWIEDLIWOŚĆ",
                "ŚWIETLISTĄ CHWAŁY SZATĘ",
                "DLA SWEGO KRÓLA DZIŚ GOTUJE SIĘ"
            ]
        },
        {
            "lines": [
                "JUŻ OBLUBIEŃCA GŁOS",
                "UMIŁOWANĄ WZYWA SWĄ",
                "GDY CZAS ZARĘCZYN KOŃCZY SIĘ",
                "WZNIEŚ OCZY SWE I SPÓJRZ",
                "JUŻ ŚWITA TEN CUDOWNY DZIEŃ",
                "GDY PAN POWRÓCI",
                "ZABRAĆ KOŚCIÓŁ SWÓJ"
            ]
        }
    ]
}