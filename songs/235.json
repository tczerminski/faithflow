{
    "id": 235,
    "title": "JEZUS, JEZUS ŚWIĘTY NAMASZCZONY PAN",
    "verses": [
        {
            "lines": [
                "JEZUS, JEZUS.",
                "ŚWIĘTY NAMASZCZONY PAN, JEZUS",
                "JEZUS, JEZUS,",
                "ŻYJE WYWYŻSZONY PAN, JEZUS"
            ]
        },
        {
            "lines": [
                "TWE IMIĘ JAK MIÓD",
                "NA USTACH MYCH,",
                "TWÓJ DUCH",
                "JEST JAK WODA DUSZY MEJ,"
            ]
        },
        {
            "lines": [
                "TWE SŁOWO",
                "JAK LAMPA U MYCH STÓP",
                "JEZU, JA KOCHAM CIĘ!",
                "JA KOCHAM!"
            ]
        }
    ]
}