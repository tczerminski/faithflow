{
    "id": 524,
    "title": "ŚWIĘTY… JEST NASZ PAN",
    "verses": [
        {
            "lines": [
                "ŚWIĘTY, ŚWIĘTY, ŚWIĘTY,",
                "ŚWIĘTY JEST NASZ PAN"
            ]
        },
        {
            "lines": [
                "GODZIEN, GODZIEN, GODZIEN",
                "GODZIEN BY PRZYJĄĆ CZEŚĆ"
            ]
        },
        {
            "lines": [
                "JEGO ŁASKA WCIĄŻ NAD NAMI JEST",
                "JEGO MIŁOŚĆ W SERCACH NASZYCH TRWA",
                "CAŁA ZIEMIA JEMU SKŁADA HOŁD",
                "I WYSŁAWIA DZIEŁA JEGO RĄK",
                "JEGO LUD DZIŚ BŁOGOSŁAWI GO",
                "ZA ZBAWIENNĄ O JEZUSIE WIEŚĆ",
                "ON UMACNIA WIERNYCH DUCHEM SWYM",
                "ON NA WIEKI WIEKÓW TRWA"
            ]
        }
    ]
}