{
    "id": 613,
    "title": "WIEDZ IZRAELU, ŻE PAN JEDYNYM BOGIEM",
    "verses": [
        {
            "lines": [
                "WIEDZ IZRAELU",
                "ŻE PAN JEDYNYM BOGIEM",
                "HALLELUJA!"
            ]
        },
        {
            "lines": [
                "I BĘDZIESZ KOCHAŁ BOGA",
                "CAŁYM SERCEM Z CAŁEJ SIŁY SWEJ",
                "ODDAWAŁ CZEŚĆ KRÓLOWI CHWAŁY",
                "TAK JAK TEGO CHCE!"
            ]
        },
        {
            "lines": [
                "NIE MA INNEGO BOGA!",
                "ANI WYBAWICIELA!",
                "CHCEMY CIĘ ZAWSZE WIELBIĆ",
                "USTAMI SWYMI OGŁASZAMY!"
            ]
        }
    ]
}