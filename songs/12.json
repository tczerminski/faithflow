{
    "id": 12,
    "title": "BĄDŹ BŁOGOSŁAWIONY",
    "verses": [
        {
            "lines": [
                "BĄDŹ BŁOGOSŁAWIONY",
                "NASZ BOŻE",
                "TY BYŁEŚ, JESTEŚ",
                "I MASZ PRZYJŚĆ",
                "BĄDŹ BŁOGOSŁAWIONY",
                "NASZ BOŻE",
                "NA WIEKI WIECZNE TRWASZ"
            ]
        },
        {
            "lines": [
                "NIEBIAŃSKI OJCZE KOCHAMY CIĘ",
                "WYWYŻSZAĆ CHCEMY",
                "ŚWIĘTE IMIĘ TWE",
                "TWE KRÓLESTWO NIECHAJ BĘDZIE",
                "UTWIERDZONE W NAS",
                "SWĄ POTĘGĘ",
                "OBJAWIAJ W LUDZIE SWYM"
            ]
        },
        {
            "lines": [
                "NAJŚWIĘTSZY BOŻE",
                "WYWYŻSZAMY CIĘ",
                "KŁADZIEMY WSZYSTKO DZIŚ",
                "PRZED TRONEM TWYM",
                "WONNOŚĆ TWOJEJ OBECNOŚCI",
                "ODCZUWAMY WCIĄŻ",
                "GDY WIELBIMY WSPANIAŁE IMIĘ TWE"
            ]
        }
    ]
}