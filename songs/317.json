{
    "id": 317,
    "title": "NIE PRZEZ SIŁĘ",
    "verses": [
        {
            "lines": [
                "NIE PRZEZ SIŁĘ,",
                "NIE PRZEZ MOC,",
                "LECZ PRZEZ DUCHA MÓWI PAN"
            ]
        },
        {
            "lines": [
                "ORĘŻ NASZEJ WALKI NIE JEST Z TEGO ŚWIATA",
                "LECZ W DUCHU MA MOC, W DUCHU MA MOC",
                "OGROM JEGO ŁASKI, POTĘGA MIŁOŚCI",
                "PONAD WSZYSTKO SĄ, PONAD WSZYSTKO SĄ"
            ]
        },
        {
            "lines": [
                "JEZUSA WIELBIMY DZIŚ",
                "I JEDNOŚĆ JEST WŚRÓD NAS",
                "MAMY ZWYCIĘSTWO,",
                "BO ON WALCZY SAM"
            ]
        }
    ]
}