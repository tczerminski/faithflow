{
    "id": 126,
    "title": "GDY DOTYKASZ MEGO ŻYCIE SIĘ",
    "verses": [
        {
            "lines": [
                "GDY DOTYKASZ MEGO ŻYCIA",
                "SIĘ RĘKĄ SWĄ",
                "TO BUDZI W SERCU MOIM PIEŚŃ",
                "PANIE KOCHAM CIĘ!"
            ]
        },
        {
            "lines": [
                "Z GŁĘBI DUCHA ŚLĘ",
                "WYZNANIE TO MIŁE CI",
                "TYŚ JEST MÓJ KRÓL!",
                "TYŚ JEST MÓJ BÓG!",
                "PANIE, KOCHAM CIĘ!"
            ]
        }
    ]
}