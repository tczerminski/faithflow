{
    "id": 561,
    "title": "TYLKO BÓG ODWAGĘ DAJE NAM",
    "verses": [
        {
            "lines": [
                "TYLKO BÓG ODWAGĘ DAJE NAM",
                "TYLKO ON",
                "DO ZWYCIĘSTW WIEDZIE NAS",
                "BO CHRYSTUS PAN",
                "POKONAŁ PIEKŁA MOC",
                "KRÓLEM JEST!"
            ]
        },
        {
            "lines": [
                "ON PRZEZWYCIĘŻYŁ ZŁEGO MOC",
                "I SWÓJ WYZWOLIŁ LUD",
                "NA SŁOWO BOGA ZADRŻY WRÓG",
                "A ZIEMIA POZNA, ŻE TO"
            ]
        }
    ]
}