{
    "id": 63,
    "title": "CHRYSTUS WYSTARCZY MI",
    "verses": [
        {
            "lines": [
                "CHRYSTUS TO MÓJ PAN",
                "I TYLKO JEGO WIELBIĘ",
                "CHOĆBY WSZYSTKO DAŁ MI ŚWIAT",
                "TYLKO W BOGU SZCZĘŚCIE MAM"
            ]
        },
        {
            "lines": [
                "PRZEZ WSZYSTKIE DNI",
                "CHCĘ ŚPIEWAĆ CI",
                "NIE COFNĘ SIĘ",
                "ZBAWIŁEŚ MNIE"
            ]
        },
        {
            "lines": [
                "CHRYSTUS WYSTARCZY MI",
                "CHRYSTUS WYSTARCZY MI",
                "WSZYSTKO CZEGO CHCĘ, MAM W TOBIE",
                "WSZYSTKO CZEGO CHCĘ"
            ]
        },
        {
            "lines": [
                "CHRYSTUS WSZYSTKIM JEST",
                "ZBAWIENIEM I RADOŚCIĄ",
                "A NADZIEJA SPEŁNI SIĘ",
                "BO NIEBO TO MÓJ DOM"
            ]
        },
        {
            "lines": [
                "PRZEZ WSZYSTKIE DNI",
                "CHCE ŚPIEWAĆ CI",
                "TY JESTEŚ TYM",
                "KTÓREGO UWIELBIAM"
            ]
        },
        {
            "lines": [
                "POSTANOWIŁEM",
                "IŚĆ ZA JEZUSEM",
                "NIE WRÓCĘ JUŻ",
                "NIE WRÓCĘ JUŻ"
            ]
        },
        {
            "lines": [
                "TEN ŚWIAT JEST ZA MNĄ",
                "A KRZYŻ PRZEDE MNĄ",
                "NIE WRÓCĘ JUŻ",
                "NIE WRÓCĘ JUŻ"
            ]
        }
    ]
}