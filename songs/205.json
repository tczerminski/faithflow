{
    "id": 205,
    "title": "JEZU – TY SŁYSZYSZ NASZE PROŚBY",
    "verses": [
        {
            "lines": [
                "JEZU TY SŁYSZYSZ NASZE PROŚBY",
                "SŁYSZYSZ MODLITWY TE",
                "OCZEKUJEMY WIĘC,",
                "ŻE UCZYNISZ TO, O CO ZGODNIE",
                "W IMIENIU TWYM PROSIMY CIĘ"
            ]
        },
        {
            "lines": [
                "WIERZYMY! DZIĘKUJEMY!",
                "PANIE JEZU WYWYŻSZAMY CIĘ",
                "PRZYJMIJ NASZĄ WDZIĘCZNOŚĆ",
                "ZA TO, CO TY CZYNISZ WŚRÓD NAS!"
            ]
        }
    ]
}