    "title": "NASZ PAN TO KRÓL",
    "verses": [
        {
            "lines": [
                "(: NASZ PAN TO KRÓL :)",
                "(: ZIEMIO RADUJ SIĘ :)",
                "NIECH RADUJE SIE LUD,",
                "ŻE WŁADA BÓG!"
            ]
        },
        {
            "lines": [
                "W PŁOMIENIACH OGNIA ZNIKNĄ",
                "ZASTĘPY NIEPRZYJACIÓŁ",
                "PAGÓRKI STOPNIEJĄ",
                "(: I ROZPŁYNĄ SIĘ JAK WOSK :)"
            ]
        },
        {
            "lines": [
                "NIEBIOSA GŁOSZĄ CHWAŁĘ",
                "I BOŻĄ SPRAWIEDLIWOŚĆ",
                "TYŚ BÓG, TYŚ NASZ KRÓL",
                "(: NIECH CIĘ SŁAWI CAŁY ŚWIAT :)"
            ]
        }
    ]
}