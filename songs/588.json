    "id": 588,
    "title": "UWIELBIAM CIĘ, JEZU",
    "verses": [
        {
            "lines": [
                "UWIELBIAM CIĘ JEZU",
                "TWA CHWAŁA NIECH NA USTACH MYCH",
                "WIECZNIE TRWA",
                "UWIELBIAM CIĘ JEZU",
                "TWA MIŁOŚĆ NIECHAJ W SERCU MYM",
                "ZAWSZE TRWA"
            ]
        },
        {
            "lines": [
                "JA PRAGNĘ CIĘ WIDZIEĆ",
                "TWEJ OBECNOŚCI SZUKAĆ CHCĘ",
                "CAŁY CZAS",
                "JA PRAGNĘ CIĘ SŁYSZEĆ",
                "JAK MÓWISZ DO MNIE Z DNIA NA DZIEŃ",
                "KOCHAM CIĘ"
            ]
        },
        {
            "lines": [
                "(: JEZU UWIELBIAM CIĘ",
                "JEZU WYWYŻSZAM CIĘ",
                "NIE ODDAM CIĘ ZA NIC NA ŚWIECIE",
                "O NIE :)"
            ]
        },
        {
            "lines": [
                "(: JA TYLKO CIEBIE JEZU MAM",
                "Z TO BĄ NIE BĘDĘ NIGDY SAM",
                "JA CAŁĄ CHWAŁĘ TOBIE DAM :)"
            ]
        }
    ]
}