{
    "id": 376,
    "title": "ON JEST MOIM ŻYCIEM",
    "verses": [
        {
            "lines": [
                "ON JEST MOIM ŻYCIEM",
                "ON JEST MOJĄ SIŁĄ",
                "ON JEST MĄ NADZIEJĄ",
                "JEZUS MÓJ PAN!"
            ]
        },
        {
            "lines": [
                "WIELBIĆ CIĘ CHCĘ ŻYCIEM MYM",
                "WIELBIĆ CIĘ CHCĘ PIEŚNIĄ MĄ",
                "ZE WSZYSTKICH MYCH SIŁ",
                "UWIELBIAĆ CIĘ CHCĘ",
                "MOJA NADZIEJA TO TY!"
            ]
        }
    ]
}