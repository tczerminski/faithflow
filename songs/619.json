    "title": "WIELKI JEST NASZ PAN",
    "verses": [
        {
            "lines": [
                "WIELKI JEST NASZ PAN",
                "POTĘŻNY JEST NASZ BÓG",
                "WIELKI JEST NASZ PAN",
                "ON STWORZYŁ CAŁY ŚWIAT"
            ]
        },
        {
            "lines": [
                "WYWYŻSZAJ BOGA DZIŚ,",
                "KRÓLOWI ODDAJ CZEŚĆ",
                "NIECH BOGU CHWAŁA BRZMI",
                "ON STWÓRCĄ WSZYSTKIEGO JEST"
            ]
        },
        {
            "lines": [
                "JEGO IMIĘ JEST WIĘKSZE",
                "NIŻ KAŻDE INNE IMIĘ",
                "JEGO MOC JEST NAJWIĘKSZA",
                "ON STWORZYŁ NIEBO I ZIEMIĘ"
            ]
        }
    ]
}