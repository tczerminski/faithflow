{
    "id": 170,
    "title": "JAK DOBRZE RAZEM JEST JEZUSA CHWALIĆ",
    "verses": [
        {
            "lines": [
                "JAK DOBRZE RAZEM",
                "JEST JEZUSA CHWALIĆ WCIĄŻ",
                "DOBRZE RAZEM",
                "JEST DZIĘKOWAĆ JEMU DZIŚ",
                "I ŚPIEWAĆ PIEŚNI KU JEGO CZCI –",
                "NAJWYŻSZYM ON!"
            ]
        },
        {
            "lines": [
                "BOGA MEGO BĘDĘ CZCIĆ",
                "I ŚPIEWAĆ MU W SERCU MYM",
                "WYWYŻSZAĆ BĘDĘ SŁAWNE IMIĘ",
                "KRÓLA CHWAŁ",
                "PRZEZ WSZYSTKIE DNI!"
            ]
        }
    ]
}