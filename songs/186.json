{
    "id": 186,
    "title": "JESTEŚ BLISKO MNIE",
    "verses": [
        {
            "lines": [
                "JESTEŚ BLISKO MNIE",
                "TĘSKNIĘ ZA DUCHEM TWYM",
                "KOCHAM KROKI TWE,",
                "WIEM JAK PUKASZ DO DRZWI",
                "PRZYCHODZISZ JAK CIEPŁY WIATR",
                "OTWIERAM SIĘ I CZUJĘ ZNÓW, ŻE"
            ]
        },
        {
            "lines": [
                "TWOJA MIŁOŚĆ JAK CIEPŁY DESZCZ",
                "TWOJA MIŁOŚĆ JAK MORZE GWIAZD",
                "ZA DNIA",
                "TWOJA MIŁOŚĆ SPRAWIA, ŻE",
                "NIESKOŃCZENIE DOBRY",
                "ŚWIĘTY DUCH OGARNIA MNIE"
            ]
        }
    ]
}