{
    "id": 348,
    "title": "O PANIE, WIELBIĘ CIĘ",
    "verses": [
        {
            "lines": [
                "O PANIE WIELBIĘ CIĘ",
                "SŁOWAMI Z SERCA WPROST",
                "A GDY ZABRAKNIE MI SŁÓW",
                "POZWÓL MILCZEĆ"
            ]
        },
        {
            "lines": [
                "POZWÓL WPATRYWAĆ SIĘ",
                "W PROMIENNĄ JASNOŚĆ TWĄ",
                "OGLĄDAĆ CHWAŁY BLASK",
                "NA TWYM OBLICZU"
            ]
        },
        {
            "lines": [
                "JESTEŚ TU – NICZEGO NIE BRAK MI",
                "GDY JESTEŚ TU",
                "OTWARTE NIEBO WIDZĘ",
                "PIĘKNY CHÓR ANIOŁÓW ŚPIEWA CI",
                "Z MILIONÓW STRUN",
                "MUZYKA NIEBA PŁYNIE"
            ]
        }
    ]
}