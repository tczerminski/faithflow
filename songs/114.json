{
    "id": 114,
    "title": "DZIĘKUJMY PANU TO NASZ BÓG I KRÓL",
    "verses": [
        {
            "lines": [
                "DZIĘKUJMY PANU, TO NASZ BÓG I KRÓL",
                "JEGO MIŁOŚĆ TRWA NA WIEKI",
                "ON DOBRY JEST, PONAD WSZYSTKIM ON",
                "JEGO MIŁOŚĆ TRWA NA WIEKI",
                "CHWAL GO, CHWAL GO"
            ]
        },
        {
            "lines": [
                "Z WYCIĄGNIĘTYM W MOCY RAMIENIEM SWYM",
                "JEGO MIŁOŚĆ TRWA NA WIEKI",
                "W NOWYM ŻYCIU, KTÓRE DAJE NAM",
                "JEGO MIŁOŚĆ TRWA NA WIEKI",
                "(: CHWAL GO, CHWAL GO :)"
            ]
        },
        {
            "lines": [
                "NA WIEKI BÓG JEST WIERNY",
                "NA WIEKI MOCNY JEST",
                "NA WIEKI BÓG JEST Z NAMI",
                "OD WIEKÓW NA WIEKI"
            ]
        },
        {
            "lines": [
                "OD WSCHODU SŁOŃCA, AŻ PO JEGO ZMIERZCH",
                "JEGO MIŁOŚĆ TRWA NA WIEKI",
                "DZIĘKI ŁASCE BOŻEJ BĘDZIEMY TRWAĆ",
                "JEGO MIŁOŚĆ TRWA NA WIEKI",
                "(: CHWAL GO, CHWAL GO :)"
            ]
        }
    ]
}