{
    "id": 580,
    "title": "U TWYCH STÓP, TO TU",
    "verses": [
        {
            "lines": [
                "U TWYCH STÓP TO TU",
                "MIEJSCE ŚWIĘTE JEST",
                "PRAGNĘ PRZYJŚĆ I SKŁONIĆ SIĘ",
                "BO MÓC Z TOBĄ BYĆ",
                "TO ZASZCZYCA MNIE",
                "WIĘC POKORNIE CHYLĘ SIĘ"
            ]
        },
        {
            "lines": [
                "W TEJ CHWILI TAK PO PROSTU",
                "CHCĘ Z TOBĄ BYĆ",
                "WPATRZONY W TWOJĄ TWARZ",
                "TY WIESZ, ŻE TAK NIEWIELE",
                "DZIŚ ZROBIĘ SAM",
                "A WIĘC PRZYJDŹ I DOTKNIJ MNIE"
            ]
        },
        {
            "lines": [
                "BO WSZYSTKO CZEGO PRAGNĘ",
                "TO Z TOBĄ BYĆ",
                "DO TWEJ ŚWIĘTOŚCI WEJŚĆ",
                "I ABY DUCH TWÓJ",
                "TERAZ OGARNĄŁ MNIE",
                "BĘDĘ ZAWSZE WIELBIĆ CIĘ"
            ]
        }
    ]
}