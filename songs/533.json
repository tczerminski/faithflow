{
    "id": 533,
    "title": "TATUŚ (ZANIM STWORZYŁ ŚWIAT)",
    "verses": [
        {
            "lines": [
                "ZANIM STWORZYŁ ŚWIAT",
                "O TOBIE MYŚLAŁ JUŻ",
                "I WSZYSTKO CZEGO CHCIAŁ,",
                "TO BYŚ SZCZĘŚLIWYM BYŁ",
                "PRZEZ WIELKĄ MIŁOŚĆ SWĄ",
                "JEZUSA – SYNA DAŁ",
                "TWÓJ TATO ZROBIŁ TO, ABYŚ MÓGŁ ŻYĆ"
            ]
        },
        {
            "lines": [
                "PRZYJDŹ DO SWEGO OJCA",
                "CHOĆ NIEWIELE MASZ",
                "ODDAJ ŻYCIE, ODDAJ SERCE,",
                "A ON ULECZY JE",
                "PRZEZ WIELKĄ MIŁOŚĆ SWĄ",
                "JEZUSA – SYNA DAŁ",
                "TWÓJ TATUŚ ZROBIŁ TO, ABYŚ MÓGŁ ŻYĆ"
            ]
        },
        {
            "lines": [
                "NIE MOŻESZ ZROBIĆ NIC,",
                "BY BARDZIEJ KOCHAŁ CIĘ",
                "NIE MOŻESZ ZROBIĆ NIC,",
                "BY WYPARŁ CIEBIE SIĘ",
                "PRZEZ MOC ŚWIĘTEGO DUCHA",
                "JEZUSA – SYNA DAŁ",
                "TWÓJ TATO ZROBIŁ TO, ABYŚ MÓGŁ ŻYĆ"
            ]
        }
    ]
}