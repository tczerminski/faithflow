{
    "id": 417,
    "title": "PANIE WIEM, ŻE NIKT",
    "verses": [
        {
            "lines": [
                "PANIE WIEM, ŻE NIKT",
                "NIE POTRAFI DOTKNĄĆ SERCA JAK TY",
                "CHOĆBYM WSZECHŚWIAT CAŁY",
                "PRZEMIERZYĆ MIAŁ",
                "TO WIEM – NIE DORÓWNA CI NIKT"
            ]
        },
        {
            "lines": [
                "TWA MIŁOŚĆ JAK WIELKIEJ RZEKI NURT",
                "I UZDROWIENIE Z TWYCH RĄK",
                "RAMIONA TWE KOJĄ CIERPIENIA BÓL",
                "PANIE, NIKT JAK TY"
            ]
        }
    ]
}