{
    "id": 396,
    "title": "PAN MÓJ CUDOWNY JEST",
    "verses": [
        {
            "lines": [
                "PAN MÓJ CUDOWNY JEST",
                "IMIĘ POTĘŻNE MA",
                "JEZUS – TO IMIĘ BRZMI",
                "JEZUS MÓJ PAN!"
            ]
        },
        {
            "lines": [
                "POTĘŻNYM BOGIEM JEST ON",
                "NAD WSZYSTKO JEGO TRON",
                "JEZUS – TO IMIĘ BRZMI",
                "JEZUS MÓJ PAN!"
            ]
        },
        {
            "lines": [
                "ON MYM PASTERZEM",
                "SKAŁĄ ODWIECZNĄ",
                "WSZECHMOCNYM BOGIEM ON JEST",
                "HOŁD MU ODDAJMY",
                "PIEŚNIĄ GO CZCIJMY",
                "JEZUS – TO IMIĘ BRZMI",
                "JEZUS MÓJ PAN!"
            ]
        }
    ]
}