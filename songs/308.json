{
    "id": 308,
    "title": "NIE LĘKAJ SIĘ",
    "verses": [
        {
            "lines": [
                "NIE LĘKAJ SIĘ, NIE LĘKAJ SIĘ",
                "JESTEM PRZY TOBIE CAŁY DZIEŃ.",
                "NIE LĘKAJ SIĘ, NIE LĘKAJ SIĘ",
                "JESTEM PRZY TOBIE, NIE BÓJ SIĘ, NIE LĘKAJ SIĘ"
            ]
        },
        {
            "lines": [
                "STRACH MIAŁ WIELKIE OCZY",
                "ALE NAS NIE ZASKOCZY",
                "ROBI GROŹNE MINY,",
                "ALE MY SIĘ NIE BOIMY"
            ]
        },
        {
            "lines": [
                "KOMARY I KRZYKI,",
                "ROBAKI I ZASTRZYKI",
                "CZY SIE PRZESTRASZYMY?",
                "NIE-MY SIĘ BOIMY."
            ]
        },
        {
            "lines": [
                "NIE TYLKO W NIEDZIELĘ",
                "(: JESTEŚ MOIM PRZYJACIELEM :)",
                "NIE TYLKO OD ŚWIĘTA,",
                "(: ALE CODZIENNIE O TOBIE PAMIĘTAM :)"
            ]
        },
        {
            "lines": [
                "NIE LĘKAJ SIE",
                "BÓG Z TOBĄ JEST",
                "ON MOCNY JEST",
                "NIE LĘKAJ SIĘ",
                "BÓG Z TOBĄ JEST"
            ]
        }
    ]
}