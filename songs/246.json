    "title": "KIMŻE JESTEM, ŻE TY MYŚLISZ",
    "verses": [
        {
            "lines": [
                "KIMŻE JESTEM,",
                "ŻE TY MYŚLISZ O MNIE",
                "ŻE MNIE SŁYSZYSZ, GDY WOŁAM CIĘ?",
                "CZY TO PRAWDA,",
                "ŻE WCIĄŻ MYŚLISZ O MNIE",
                "ŻE MNIE KOCHASZ?",
                "TO NIEZWYKŁE"
            ]
        },
        {
            "lines": [
                "(: BÓG PRZYJACIELEM MYM :) X3",
                "ON PRZY MNIE JEST"
            ]
        },
        {
            "lines": [
                "BOŻE WIELKI,",
                "BOŻE CHWAŁY,",
                "JESTEŚ BLISKO MNIE"
            ]
        }
    ]
}