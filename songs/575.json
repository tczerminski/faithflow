{
    "id": 575,
    "title": "TYŚ MOIM ŻYCIEM",
    "verses": [
        {
            "lines": [
                "TYŚ MOIM ŻYCIEM",
                "DZIEDZICTWEM MYM",
                "TYŚ BOGACTWEM I NAGRODĄ MĄ",
                "PRAGNIENIEM MYM I PEŁNIĄ MĄ",
                "(: WSZYSTKIM DLA MNIE DZIŚ JESTEŚ TY :)"
            ]
        },
        {
            "lines": [
                "PONAD SYTOŚĆ DNI",
                "PANIE TY WYSTARCZASZ MI",
                "SWĄ DOBROCIĄ SWĄ MIŁOŚCIĄ",
                "SYCISZ MNIE"
            ]
        },
        {
            "lines": [
                "TWA RADOŚĆ JEST",
                "NAPOJEM DUSZY MEJ",
                "WSZYSTKO TO",
                "W CZYM ZAWIÓDŁ ŚWIAT",
                "W TOBIE PANIE DZISIAJ MAM"
            ]
        },
        {
            "lines": [
                "PANIE DAŁEŚ MI",
                "WSZYSTKO CO POSIADAM DZIŚ",
                "BŁOGOSŁAWISZ MNIE BOGACTWEM",
                "SWOIM WCIĄŻ"
            ]
        },
        {
            "lines": [
                "TYŚ NAGRODĄ MĄ",
                "PRAGNIENIEM JESTEŚ MYM",
                "MIŁOŚĆ TWOJA NIE MA GRANIC",
                "MIŁOŚĆ TWA ZADZIWIA MNIE"
            ]
        }
    ]
}