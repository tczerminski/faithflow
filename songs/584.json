{
    "id": 584,
    "title": "UKRYJ MNIE POD SKRZYDŁA SWE",
    "verses": [
        {
            "lines": [
                "UKRYJ MNIE",
                "POD SKRZYDŁA SWE",
                "OSŁOŃ MNIE",
                "POTĘŻNĄ RĘKĄ TWĄ"
            ]
        },
        {
            "lines": [
                "GDY W MYM ŻYCIU BURZA ZERWIE SIĘ",
                "TY SAM PONAD NIĄ UNIESIESZ MNIE",
                "OJCZE TYŚ JEST KRÓLEM WSZYSTKICH WÓD",
                "NIE BOJĘ SIĘ. TYŚ JEST MÓJ BÓG!"
            ]
        },
        {
            "lines": [
                "JEDYNIE W NIM",
                "WYTCHNIENIE MAM",
                "UFAM JEMU,",
                "ON W CISZY WZMACNIA MNIE"
            ]
        }
    ]
}