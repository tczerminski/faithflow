{
    "id": 556,
    "title": "TY JESTEŚ SKAŁĄ ZBAWIENIA MEGO",
    "verses": [
        {
            "lines": [
                "TY JESTEŚ SKAŁĄ ZBAWIENIA MEGO",
                "W TOBIE MA SIŁA I MOC",
                "TYŚ MĄ NADZIEJĄ",
                "I MYM NATCHNIENIEM",
                "WZNOSZĘ DO CIEBIE SWÓJ GŁOS"
            ]
        },
        {
            "lines": [
                "PANIE WIERZĘ CI!",
                "TAK WIERZĘ CI,",
                "BO TWA MIŁOŚĆ WIERNA JEST",
                "TYŚ POMOCĄ MĄ",
                "W POTRZEBACH MYCH",
                "PANIE PRZY TOBIE CHCĘ BYĆ"
            ]
        }
    ]
}