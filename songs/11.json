{
    "id": 11,
    "title": "ARMII KRZYK",
    "verses": [
        {
            "lines": [
                "SŁYSZĘ TŁUM,",
                "GŁOŚNY KRZYK",
                "ARMII TWEJ",
                "NIE ZATRZYMA JUŻ NIC!",
                "BUDZI SIĘ",
                "WIELKI DZIEŃ",
                "WIDZĘ JAK",
                "IDĄ, BY ZDOBYĆ ŚWIAT"
            ]
        },
        {
            "lines": [
                "ZNAJĄ ŹRÓDŁO SWEJ SIŁY",
                "MAJĄ CEL, WIERZĄ MU",
                "NIE PODDADZĄ SIĘ NIGDY",
                "ZIEMIA DRŻY OD ICH STÓP",
                "SKAŁĄ ICH ŻYCIA CHRYSTUS",
                "KRÓLÓW KRÓL! PANÓW PAN!",
                "TYLKO JEMU PODDANI",
                "TO DLA NIEGO CHCĄ ŻYĆ!"
            ]
        }
    ]
}