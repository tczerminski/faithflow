{
    "id": 160,
    "title": "JA MOGĘ ROZMAWIAĆ Z BOGIEM",
    "verses": [
        {
            "lines": [
                "JA MOGĘ ROZMAWIAĆ Z BOGIEM",
                "I Z TEGO CIESZĘ SIĘ",
                "CHOĆ ZARAZ CI OPOWIEM",
                "JAK MOŻESZ MODLIĆ SIĘ"
            ]
        },
        {
            "lines": [
                "ZNAJDŹ CICHE MIEJSCE",
                "OTWÓRZ SWE SERCE",
                "NADSTAW UCHA I CO?",
                "I SŁUCHAJ DUCHA"
            ]
        },
        {
            "lines": [
                "BO MODLITWA NIE JEST TRUDNA, NIE",
                "BO MODLITWA NIE JEST NUDNA, NIE",
                "JEZUS BARDZO CIESZY SIĘ",
                "KIEDY SŁYSZY SŁOWO TWE"
            ]
        }
    ]
}