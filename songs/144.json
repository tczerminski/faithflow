    "verses": [
        {
            "lines": [
                "HEJ JEZU KRÓLEM TYŚ",
                "ŻYCIE TWE UWALNIA MNIE",
                "BĘDĘ SŁAWIĆ CAŁY DZIEŃ",
                "DOSKONAŁE DROGI TWE."
            ]
        },
        {
            "lines": [
                "HEJ JEZU PANEM TYŚ",
