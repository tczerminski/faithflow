{
    "id": 248,
    "title": "KOCHAM CIĘ, PANIE, KOCHAM CIĘ",
    "verses": [
        {
            "lines": [
                "KOCHAM CIĘ PANIE, KOCHAM CIĘ",
                "TYŚ JEDYNIE BOGIEM MOIM",
                "PANEM MOIM JEST"
            ]
        },
        {
            "lines": [
                "ZMIENIŁEŚ ŻYCIE ME",
                "OBMYŁEŚ MNIE SWĄ KRWIĄ",
                "DAŁEŚ MI POKÓJ SWÓJ",
                "NAZWAŁEŚ DZIECKIEM SWYM",
                "UWIELBIAM PANIE CIĘ",
                "WYSŁAWIAM IMIĘ TWE",
                "WZNOSZĘ SWE RĘCE WZWYŻ",
                "TYŚ GODZIEN CHWAŁY JEST"
            ]
        }
    ]
}