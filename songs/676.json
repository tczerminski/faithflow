{
    "id": 676,
    "title": "ZMIENIŁEŚ W TANIEC",
    "verses": [
        {
            "lines": [
                "ZMIENIŁEŚ W TANIEC",
                "MOJĄ SKARGĘ I PŁACZ",
                "NIE MOGĘ JUŻ MILCZEĆ",
                "BĘDĘ CI ŚPIEWAĆ",
                "WYPEŁNIONY RADOŚCIĄ TWĄ"
            ]
        },
        {
            "lines": [
                "TWA UZDRAWIAJĄCA DŁOŃ UKOJENIE DAŁA MI",
                "KIEDY CZUŁEM WIELKI BÓL",
                "TY OTARŁEŚ MOJE ŁZY"
            ]
        },
        {
            "lines": [
                "ME PRZYGNĘBIENIE ZNIKA, GDY CZUJĘ TWĄ BLISKOŚĆ",
                "BLASK TWEJ MIŁOŚCI SPRAWIA, ŻE",
                "TERAZ WIDZĘ WSZYSTKO W NOWYM ŚWIETLE"
            ]
        },
        {
            "lines": [
                "TWÓJ ŚWIĘTY GNIEW",
                "TYLKO PRZEZ CHWILĘ TRWA",
                "LECZ TWA ŁASKA OD DZIŚ",
                "BĘDZIE TRWAĆ DO KOŃCA MOICH DNI"
            ]
        }
    ]
}