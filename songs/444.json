{
    "id": 444,
    "title": "PRZED TRONEM ŁASKI STAJĘ",
    "verses": [
        {
            "lines": [
                "PRZED TRONEM ŁASKI STAJĘ",
                "BY PRZYJĄĆ POMOC TWĄ",
                "PANIE POTRZEBUJĘ CIĘ",
                "SZUKAM TWARZY TWEJ"
            ]
        },
        {
            "lines": [
                "BOŻE CHWAŁY, BOŻE MOCY,",
                "MAJESTATU"
            ]
        },
        {
            "lines": [
                "OJCZE DZIŚ PRZYCHODZĘ",
                "PRZEZ CHRYSTUSA KREW",
                "SKŁADAM CI W OFIERZE",
                "CAŁE ŻYCIE SWE"
            ]
        }
    ]
}