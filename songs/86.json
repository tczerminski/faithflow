{
    "id": 86,
    "title": "DAŁEŚ ZA MNIE ŻYCIE DOBRY JEZU",
    "verses": [
        {
            "lines": [
                "DAŁEŚ ŻYCIE ZA MNIE DOBRY JEZU",
                "WYZWOLIŁEŚ MNIE PRZEZ KRZYŻ",
                "DZIŚ WIELBIĘ CIĘ I SKŁADAM HOŁD",
                "W OFIERZE DAJĘ ŻYCIE SWE"
            ]
        },
        {
            "lines": [
                "PROSZĘ POMÓŻ ZROZUMIEĆ MYŚLI TWE",
                "WESPRZYJ MNIE W CHWILI ZŁEJ",
                "ZAMIEŃ SMUTEK W RADOŚCI ŁZY",
                "ZAPAL OGIEŃ W SERCU MYM"
            ]
        },
        {
            "lines": [
                "ODDAJĘ CI MOJE SERCE DZIŚ",
                "PRZYJMIJ CHWAŁY MOJĄ PIEŚŃ",
                "ODDAJĘ CI MARZENIA PLANY ME",
                "DZIŚ DLA CIEBIE PRAGNĘ ŻYĆ"
            ]
        }
    ]
}