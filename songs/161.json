{
    "id": 161,
    "title": "JA NIE NARZEKAM",
    "verses": [
        {
            "lines": [
                "JA NIE NARZEKAM",
                "CHOCIAŻ WIELE TU NIE MAM",
                "IZDEBKĘ MAŁĄ I WIĘCEJ NIC,",
                "ALE W WIECZNOŚCI,",
                "W MEJ OJCZYŹNIE NIEBIAŃSKIEJ",
                "BĘDĘ MIEĆ PAŁAC, CO ZŁOTEM LŚNI!"
            ]
        },
        {
            "lines": [
                "TAK! JA MAM PAŁAC",
                "TAM POZA GÓRAMI",
                "W TYM JASNYM KRAJU",
                "GDZIE MŁODOŚĆ WCIĄŻ TRWA",
                "GDZIE ŁZY NIE PŁYNĄ",
                "WSZYSTKIE TROSKI PRZEMINĄ",
                "TAM MI MÓJ ZBAWCA KORONĘ DA"
            ]
        },
        {
            "lines": [
                "CHOĆ TUTAJ CZĘSTO",
                "ZMĘCZONY, ZNĘKANY",
                "I TAK JAK PROROK",
                "POD GŁOWĄ MA GŁAZ"
            ]
        },
        {
            "lines": [
                "JA SIĘ NIE MARTWIĘ",
                "BO W NIEBIE DOSTANĘ",
                "SWÓJ WŁASNY PAŁAC",
                "NA WIECZNY CZAS"
            ]
        },
        {
            "lines": [
                "WIĘC MI NIE WSPÓŁCZUJ",
                "CHOĆ WYDAJĘ SIĘ BIEDNY",
                "JA PIELGRZYMUJĘ DO GÓRNYCH STRON",
                "JA SZUKAM MIASTA",
                "ZE ZŁOTYMI DROGAMI",
                "TAM JA DOSTANĘ KORONĘ I DOM!"
            ]
        }
    ]
}