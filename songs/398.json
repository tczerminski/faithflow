{
    "id": 398,
    "title": "PAN NASZ WIELKI JEST",
    "verses": [
        {
            "lines": [
                "PAN NASZ WIELKI JEST,",
                "NIECH BRZMI MU PIEŚŃ CHWAŁY.",
                "PAN NASZ DOBRY JEST,",
                "BŁOGOSŁAW DUSZO MU."
            ]
        },
        {
            "lines": [
                "ŁASKAWY DOBRY JEST PAN NASZ PAN",
                "I NIEPOJĘTY W MIŁOŚCI SWEJ NA WIEKI.",
                "JEGO SPRAWIEDLIWOŚĆ TRWA.",
                "BŁOGOSŁAWMY JEGO IMIĘ.",
                "DO WSZYSTKICH KTÓRZY WZYWAJĄ GO,",
                "ON W ŁASCE SWOJEJ PRZYBLIŻA SIĘ,",
                "KRÓLESTWO JEGO NA WIEKI TRWA.",
                "BŁOGOSŁAWMY JEGO IMIĘ."
            ]
        }
    ]
}