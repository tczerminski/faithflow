    "title": "DZIĘKUJĘ CI JEZU",
    "verses": [
        {
            "lines": [
                "(: DZIĘKUJĘ CI JEZU :) X3",
                "TAK BARDZO CI DZIĘKUJĘ",
                "TAK BARDZO JESTEM",
                "WDZIĘCZNY CI"
            ]
        },
        {
            "lines": [
                "ZA GOLGOTĘ ZA TWOJĄ KREW",
                "JEZU JA DZIĘKUJĘ CI",
                "ŻE WZIĄŁEŚ WINĘ I KARĘ MĄ",
                "DLA CIEBIE BĘDĘ ŻYŁ"
            ]
        }
    ]
}