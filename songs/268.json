{
    "id": 268,
    "title": "MAJESTAT TWÓJ",
    "verses": [
        {
            "lines": [
                "MAJESTAT TWÓJ",
                "NIE WSTRZYMAŁ CIĘ",
                "KORONY SWEJ WYRZEKŁEŚ SIĘ",
                "WYDANY BYŁEŚ W RĘCE TYCH,",
                "KTÓRYCH STWORZYŁEŚ"
            ]
        },
        {
            "lines": [
                "MÓJ GRZECH,",
                "MÓJ BRUD ZMAZAŁEŚ SAM",
                "KIEDYŚ ZMARŁ I ZMARTWYCHWSTAŁ",
                "TERAZ RZĄDZISZ TY",
                "NAD NIEBO WYWYŻSZONY"
            ]
        },
        {
            "lines": [
                "TYLKO CIEBIE PANIE",
                "WIELBIĆ CHCĘ",
                "TYLKO TY ZMIENIŁEŚ",
                "SERCE ME",
                "PO WIEKI WIECZNE",
                "BĘDĘ KOCHAĆ CIĘ"
            ]
        },
        {
            "lines": [
                "BO TYLKO TY",
                "ZDECYDOWAŁEŚ SIĘ",
                "UMRZEĆ ZA MNIE I WYZWOLIĆ MNIE",
                "TOBIE PANIE DZISIAJ",
                "ŚPIEWAM W UWIELBIENIU"
            ]
        }
    ]
}