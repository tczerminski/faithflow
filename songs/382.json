{
    "id": 382,
    "title": "ON ŻYJE, TAK, NA WIEKI ŻYJE PAN",
    "verses": [
        {
            "lines": [
                "ON ŻYJE! TAK NA WIEKI ŻYJE PAN!",
                "MÓJ JEZUS ŻYJE DZIŚ",
                "NA WIEKI ŻYJE ON",
                "ON ŻYJ E! TAK NA WIEKI ŻYJE PAN",
                "I ZAWSZE BĘDZIE ŻYŁ!"
            ]
        },
        {
            "lines": [
                "CZEŚĆ ALLELUJA! CZEŚĆ ALLELUJA!",
                "MÓJ JEZUS ŻYJE DZIŚ",
                "NA WIEKI ŻYJE ON!",
                "CZEŚĆ ALLELUJA! CZEŚĆ ALLELUJA!",
                "NA ZAWSZE BĘDZIE ŻYŁ"
            ]
        }
    ]
}