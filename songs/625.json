{
    "id": 625,
    "title": "WIEM – NIE MUSZĘ SIĘ BAĆ (TY OTACZASZ MNIE)",
    "verses": [
        {
            "lines": [
                "TY OTACZASZ MNIE PIEŚNIĄ CHWAŁY SWEJ,",
                "SWĄ MELODIĄ W SERCU GRASZ.",
                "WYZWOLENIA PIEŚŃ Z SIDEŁ WOKÓŁ MNIE",
                "BRZMI, AŻ ZNIKNIE KAŻDY LĘK."
            ]
        },
        {
            "lines": [
                "WIEM – NIE MUSZĘ SIĘ JUŻ BAĆ.",
                "JESTEM DZIECKIEM TWYM."
            ]
        },
        {
            "lines": [
                "TY WYBRAŁEŚ MNIE W ŁONIE MATKI MEJ,",
                "SAM NADAŁEŚ IMIĘ MI.",
                "DZIĘKI ŁASCE TWEJ NARODZIŁEM SIĘ,",
                "ME ŻYCIE W TWOJEJ KRWI."
            ]
        },
        {
            "lines": [
                "OOOOO! OOOOO!"
            ]
        },
        {
            "lines": [
                "ROZDZIELASZ MORZE ABYM PRZEZ NIE PRZESZEDŁ.",
                "W MIŁOŚCI TONIE KAŻDY LĘK.",
                "TY OCALIŁEŚ MNIE, WIĘC MOGĘ ŚPIEWAĆ:",
                "\"JESTEM DZIECKIEM TWYM\"."
            ]
        }
    ]
}