{
    "id": 672,
    "title": "ZIEMIA",
    "verses": [
        {
            "lines": [
                "ZIEMIA, KTÓRĄ MI DAJESZ",
                "NIE JEST FIKCJĄ, ANI BAJKĄ",
                "WOLNOŚĆ, KTÓRĄ MAM W TOBIE",
                "JEST PRAWDZIWA"
            ]
        },
        {
            "lines": [
                "WSZYSTKIE GÓRY NA DRODZE",
                "MUSZĄ, MUSZĄ USTĄPIĆ,",
                "BO WIARA GÓRY PRZENOSI,",
                "A JA WIERZĘ TOBIE"
            ]
        },
        {
            "lines": [
                "BĘDĘ TAŃCZYĆ",
                "PRZED TWYM TRONEM",
                "I ODDAM TOBIE CHWAŁĘ",
                "I NIC JUŻ NIE ZAMKNIE MI UST",
                "ŻADEN MUR I ŻADNA ŚCIANA",
                "NAJWIĘKSZA NAWET TAMA",
                "JUŻ NIE,",
                "NIE ZATRZYMA MNIE JUŻ"
            ]
        },
        {
            "lines": [
                "WIĘKSZY, WIĘKSZY JEST",
                "WE MNIE TEN,",
                "KTÓRY MNIE UMACNIA",
                "ŻADEN GOLIAT",
                "NIE MOŻE Z NIM RÓWNAĆ SIĘ"
            ]
        }
    ]
}