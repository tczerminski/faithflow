{
    "id": 261,
    "title": "KU TOBIE WZNOSZĘ RĘCE ME WZWYŻ",
    "verses": [
        {
            "lines": [
                "KU TOBIE WZNOSZĘ RĘCE ME WZWYŻ",
                "KU TOBIE PODNOSZĘ MÓJ WZROK",
                "CHWALĄC CIĘ WYŚPIEWAĆ CHCĘ",
                "WDZIĘCZNOŚĆ SWĄ, MIŁOŚĆ SWĄ"
            ]
        },
        {
            "lines": [
                "JA WIEM TY, JEZU MIŁUJESZ MNIE",
                "NA KRZYŻU TWOIM ZOSTAŁ MÓJ GRZECH",
                "DLATEGO CHWALĄC CIĘ WYŚPIEWAĆ CHCĘ",
                "WDZIĘCZNOŚĆ SWĄ, MIŁOŚĆ SWĄ"
            ]
        },
        {
            "lines": [
                "ŚPIEWAM HALLELUJA, HALLELUJA"
            ]
        }
    ]
}