    "title": "PÓJDĘ Z TOBĄ TAM",
    "verses": [
        {
            "lines": [
                "PÓJDĘ Z TOBĄ TAM",
                "GDZIE NIE DOSZEDŁ JESZCZE NIKT",
                "PÓJDĘ Z TOBĄ I",
                "CHCĘ POWIERZYĆ WSZYSTKO CI",
                "PÓJDĘ Z TOBĄ TAM",
                "GDZIE TWA WOLA SPEŁNI SIĘ",
                "PROSZĘ UŻYJ MNIE",
                "JA PODDAJĘ SIĘ TO BIE"
            ]
        },
        {
            "lines": [
                "PODDAJE WSZYSTKO TOBIE",
                "WEŹ MNIE TAKIEGO JAK JESTEM",
                "PROSZĘ UŻYJ MNIE",
                "ZBAWIĆ TO POKOLENIE,",
                "WYWYŻSZYĆ CIEBIE NAD POLSKĄ",
                "PROSZĘ UŻYJ MNIE"
            ]
        },
        {
            "lines": [
                "PROSZĘ UŻYJ MNIE,",
                "JA PODDAJĘ SIĘ TOBIE"
            ]
        }
    ]
}