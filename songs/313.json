{
    "id": 313,
    "title": "NIE MA ZA TRUDNYCH SPRAW",
    "verses": [
        {
            "lines": [
                "NIE MA ZA TRUDNYCH SPRAW",
                "NIE MA ZBYT BŁAHYCH TEŻ",
                "DLA CIEBIE PANIE",
                "TY PRZECIEŻ WSZYTKO WIESZ"
            ]
        },
        {
            "lines": [
                "KAŻDĄ POTRZEBĘ ZNASZ",
                "SŁYSZYSZ MÓJ ŚMIECH I PŁACZ",
                "DLA CIEBIE PANIE",
                "TO PRZECIEŻ WAŻNE JEST"
            ]
        },
        {
            "lines": [
                "DLA CIEBIE WSZYSTKO JEST MOŻLIWE",
                "I NIE MA RZECZY BEZ ZNACZENIA",
                "DLATEGO MODLE SIE DO CIEBIE",
                "DOBRY PANIE"
            ]
        },
        {
            "lines": [
                "I WIEM ŻE ZANIM POWIEM SŁOWO",
                "TY BOŻE ZNASZ JUŻ CAŁE ZDANIE",
                "UWIELBIAM CIĘ TWOJA WOLA",
                "NIECH SIĘ STANIE"
            ]
        }
    ]
}