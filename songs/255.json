{
    "id": 255,
    "title": "KTO JEST TYM KRÓLEM CHWAŁY",
    "verses": [
        {
            "lines": [
                "KTO JEST TYM KRÓLEM CHWAŁY",
                "ON MIŁOŚCIĄ DARZY SWĄ",
                "I DOTYKA MNIE GŁĘBOKO",
                "CICHYM GŁOSEM SWOICH SŁÓW"
            ]
        },
        {
            "lines": [
                "ME SUMIENIE PRZYPOMINA",
                "PRZEBACZENIA JEGO DAR",
                "KTO JEST TYM KRÓLEM CHWAŁY",
                "ŻE TO WSZYSTKO SAM MI DAŁ"
            ]
        },
        {
            "lines": [
                "KTO KRÓLUJE WŚRÓD ANIOŁÓW",
                "I POKOJU KSIĘCIEM JEST",
                "OBJAWIONYCH RZECZY PANEM",
                "I TAJEMNIC NIEBA TEŻ"
            ]
        },
        {
            "lines": [
                "MÓJ DUCH PRAGNIE JEGO ŁASKI",
                "BEZ NIEJ JUŻ NIE MOGĘ ŻYĆ",
                "KTO JEST TYM KRÓLEM CHWAŁY",
                "SYN CZŁOWIECZY, BOŻY SYN"
            ]
        },
        {
            "lines": [
                "MA IMIĘ JEZUS",
                "DROGI JEZUS",
                "PAN WSZECHMOCNY",
                "W MOIM SERCU",
                "CHWAŁY KRÓL"
            ]
        },
        {
            "lines": [
                "KTO JEST TYM KRÓLEM CHWAŁY",
                "Z MOCĄ W MAJESTACIE SWYM",
                "MĄDROŚĆ JEGO NIE ZNA GRANIC",
                "JEGO ŁASKA CIĄGLE TRWA"
            ]
        },
        {
            "lines": [
                "ON JEST PANEM NIEBA, ZIEMI",
                "WSZYSTKO DZIEŁEM JEGO RĄK",
                "TO MÓJ PAN JEST KRÓLEM CHWAŁY",
                "WSZYSTKIM DZIŚ",
                "JEST DLA MNIE ON!"
            ]
        }
    ]
}