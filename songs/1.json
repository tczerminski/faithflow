{
    "id": 1,
    "title": "ALBOWIEM DZIECIĘ ZRODZIŁO SIĘ",
    "verses": [
        {
            "lines": [
                "ALBOWIEM DZIECIĘ ZRODZIŁO SIĘ",
                "I NAM WSZYSTKIM SYN JEST DANY",
                "SPOCZNIE WSZELKA WŁADZA",
                "NA RAMIENIU JEGO!"
            ]
        },
        {
            "lines": [
                "BĘDĄ GO ZWAĆ:",
                "DZIWNY KRÓL! RADNY PAN!",
                "WSZECHMOCNY BÓG!"
            ]
        },
        {
            "lines": [
                "ON JEST ODWIECZNYM OJCEM",
                "I POKOJU KSIĘCIEM ON"
            ]
        }
    ]
}