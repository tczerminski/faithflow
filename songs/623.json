{
    "id": 623,
    "title": "WIELKIE I GROŹNE (JESTEŚ ŚWIĘTY)",
    "verses": [
        {
            "lines": [
                "WIELKIE I GROŹNE JEST IMIĘ TWOJE, ZIEMIA DRŻY",
                "KIEDY ZASIADASZ NA SWOIM TRONIE.",
                "JAKŻE POTĘŻNY, JAK SPRAWIEDLIWY JESTEŚ TY",
                "KRÓLU WSZECHŚWIATA BĄDŹ WYWYŻSZONY."
            ]
        },
        {
            "lines": [
                "JESTEŚ ŚWIĘTY, ŚWIĘTY, ŚWIĘTY",
                "KLĘKAMY U TWYCH STÓP"
            ]
        },
        {
            "lines": [
                "JESTEŚ ŚWIĘTY, ŚWIĘTY, ŚWIĘTY",
                "NASZ BÓG"
            ]
        },
        {
            "lines": [
                "JESTEŚ KRÓLEM, WIELKIM KRÓLEM",
                "KLĘKA NIEBO, ZIEMIA DRŻY",
                "ODDAJEMY POKŁON TOBIE",
                "ŚWIĘTY, ŚWIĘTY, ŚWIĘTY"
            ]
        }
    ]
}