{
    "id": 301,
    "title": "NASZ BÓG POTĘŻNY JEST",
    "verses": [
        {
            "lines": [
                "NASZ BÓG POTĘŻNY JEST",
                "W MOCY SWEJ PANUJE NAD NAMI",
                "Z NIEBA RZĄDZI ON",
                "W MIŁOŚCI, MĄDROŚCI SWEJ"
            ]
        },
        {
            "lines": [
                "TY BYŁEŚ POWODEM,",
                "ŻE ON PRZELAŁ SWOJĄ KREW",
                "(NASZ BÓG POTĘŻNY JEST)",
                "JEGO POWRÓT JEST BLIŻSZY",
                "Z KAŻDYM DNIEM",
                "(NASZ BÓG POTĘŻNY JEST)"
            ]
        },
        {
            "lines": [
                "WIĘC LEPIEJ DLA CIEBIE,",
                "GDY UWIERZYSZ MI",
                "ŻE ON ZMAZAŁ GRZECH,",
                "MNÓSTWO TWOICH WIN NA SIEBIE WZIĄŁ",
                "WIĘC PAMIĘTAJ, ŻE",
                "NASZ BÓG POTĘŻNY JEST"
            ]
        },
        {
            "lines": [
                "ON STWORZYŁ ŚWIATŁOŚĆ",
                "PRZEMÓWIŁ DO CIEMNOŚCI",
                "(NASZ BÓG POTĘŻNY JEST)",
                "NA BEZGWIEZDNYM NIEBIE",
                "ROZSIAŁ MILION GWIAZD",
                "(NASZ BÓG POTĘŻNY JEST)"
            ]
        },
        {
            "lines": [
                "A ONI TAM OSACZYLI GO",
                "OSĄDZILI KIEDY ON MIŁOŚCIĄ SWĄ",
                "OBDARZYŁ NAS,",
                "WIĘC PAMIĘTAJ, ŻE",
                "NASZ BÓG POTĘŻNY JEST"
            ]
        }
    ]
}