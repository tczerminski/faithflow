{
    "id": 639,
    "title": "WSPÓŁCZUCIA KAŻDY PRAGNIE",
    "verses": [
        {
            "lines": [
                "WSPÓŁCZUCIA KAŻDY PRAGNIE",
                "MIŁOŚCI NIEZAWODNEJ",
                "NIECH SPŁYWA ŁASKA TWA",
                "NIECH PRZEBACZENIE PRZYJDZIE",
                "DOBROĆ ZBAWICIELA,",
                "NADZIEI ŚWIATA"
            ]
        },
        {
            "lines": [
                "ZBAWCA, ON PORUSZA GÓRY",
                "MÓJ BÓG MA ZBAWIENIA MOC",
                "ON MA ZBAWIENIA MOC",
                "NA ZAWSZE SPRAWCA ODKUPIENIA",
                "ON POWSTAŁ, ZWYCIĘŻYŁ GRÓB",
                "JEZUS ZWYCIĘŻYŁ GRÓB"
            ]
        },
        {
            "lines": [
                "WIĘC WEŹ MNIE JAKIM JESTEM",
                "Z LĘKAMI, PORAŻKAMI",
                "WYPEŁNIJ ŻYCIE ME",
                "ODDAJE MOJE ŻYCIE",
                "WSZYSTKO TO, W CO WIERZĘ",
                "ODDAJĘ TOBIE"
            ]
        },
        {
            "lines": [
                "ZAJAŚNIEJMY NIECH ZOBACZY ŚWIAT,",
                "ŻE ŚPIEWAMY",
                "DLA TWEJ CHWAŁY,",
                "ZMARTWYCHWSTAŁY KRÓL, JEZUS"
            ]
        }
    ]
}