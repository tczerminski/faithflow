{
    "id": 387,
    "title": "OTOM JEST, O PANIE POŚLIJ MNIE",
    "verses": [
        {
            "lines": [
                "OTOM JEST, O PANIE POŚLIJ MNIE",
                "ŻYCIEM SWYM TOBIE SŁUŻYĆ CHCĘ."
            ]
        },
        {
            "lines": [
                "CZEKAJĄ JUŻ BIAŁE ŁANY,",
                "LECZ ROBOTNIKÓW MAŁO WCIĄŻ",
                "WIĘC TWYM ŻNIWIARZEM",
                "PRAGNĘ BYĆ. O PANIE,",
                "DUSZ LUDZKICH ZŁOTE KŁOSY ŻĄĆ"
            ]
        },
        {
            "lines": [
                "GOTOWY JUŻ JEST NASZ NARÓD,",
                "BY POZNAĆ WIELKĄ BOŻĄ MOC.",
                "BÓG SZUKA TYCH, CO Z CAŁEJ SIŁY",
                "PRAGNĄ W ZWYCIĘSTWIE JEGO",
                "UDZIAŁ WZIĄĆ."
            ]
        },
        {
            "lines": [
                "NIE BĄDŹMY JAK SÓL ZWIETRZAŁA",
                "NIECH ŚWIATŁO NASZE JASNO LŚNI.",
                "WIERNYMI DZIŚ OKAŻMY SIĘ SŁUGAMI.",
                "NIECH BOŻA MOC OBJAWI SIĘ"
            ]
        }
    ]
}