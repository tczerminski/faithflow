{
    "id": 133,
    "title": "GDY WPATRUJĘ SIĘ",
    "verses": [
        {
            "lines": [
                "GDY WPATRUJĘ SIĘ",
                "W TWĄ ŚWIĘTĄ TWARZ",
                "GDY ROZMYŚLAM O MIŁOŚCI TWEJ",
                "KIEDY TO, CO WOKÓŁ MNIE",
                "W ŚWIETLE TWYM ROZPRASZA SIĘ"
            ]
        },
        {
            "lines": [
                "GDY DOTYKAM SOBĄ SERCA TWEGO",
                "GDY MĄ WOLĘ SKŁADAM",
                "U TWYCH STÓP",
                "KIEDY TO, CO WOKÓŁ MNIE",
                "W ŚWIETLE TWYM ROZPRASZA SIĘ"
            ]
        },
        {
            "lines": [
                "UWIELBIAM CIĘ! UWIELBIAM CIĘ!",
                "BO MEGO ŻYCIA CEL –",
                "TO WIELBIĆ CIĘ!"
            ]
        }
    ]
}