{
    "id": 110,
    "title": "DZIĘKI JEZU, DROGI JEZU",
    "verses": [
        {
            "lines": [
                "DZIĘKI JEZU! DROGI JEZU",
                "DZIĘKI CI, ŻE KOCHASZ MNIE"
            ]
        },
        {
            "lines": [
                "NA GOLGOCIE ZA MNIE ZMARŁEŚ",
                "DZIĘKI CI, ŻE KOCHASZ MNIE"
            ]
        },
        {
            "lines": [
                "ZMARTWYCHWSTAŁEŚ!",
                "ŻYJESZ WE MNIE",
                "DZIĘKI CI, ŻE KOCHASZ MNIE"
            ]
        },
        {
            "lines": [
                "WKRÓTCE PRZYJDZIESZ,",
                "STĄD MNIE WEŹMIESZ",
                "DZIĘKI CI, ŻE KOCHASZ MNIE"
            ]
        },
        {
            "lines": [
                "ALLELUJA! ALLELUJA!",
                "DZIĘKI CI, ŻE KOCHASZ MNIE"
            ]
        }
    ]
}