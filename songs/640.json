{
    "id": 640,
    "title": "WSZECHMOCNY BÓG",
    "verses": [
        {
            "lines": [
                "WSZECHMOCNY BÓG ZBAWICIEL MÓJ",
                "TO W CIENIU TWYM SCHRONIENIE MAM",
                "JEDYNIE IMIĘ JEZUS",
                "NAD WSZELKĄ MOCĄ JEST"
            ]
        },
        {
            "lines": [
                "ME STOPY NA TWYM SŁOWIE SĄ",
                "I W TOBIE NIE ZACHWIEJĘ SIĘ",
                "JEDYNIE TYŚ NADZIEJĄ MĄ",
                "MÓJ BÓG – ZBAWIENIE ME"
            ]
        },
        {
            "lines": [
                "USTAMI SWYMI WIELBIĘ CIĘ",
                "TWE SŁOWO ŻYJE W SERCU MYM",
                "DZIŚ NOWĄ PIEŚNIĄ",
                "CHCĘ WIELBIĆ I BŁOGOSŁAWIĆ CIĘ"
            ]
        },
        {
            "lines": [
                "RADOŚCIĄ SWĄ WYPEŁNIASZ MNIE",
                "I W TOBIE ROZKOSZ MOJA JEST",
                "DZIŚ NOWĄ PIEŚNIĄ",
                "CHCĘ WIELBIĆ I BŁOGOSŁAWIĆ CIĘ"
            ]
        },
        {
            "lines": [
                "GDY JESTEM SŁABY, WZMACNIASZ MNIE",
                "DAJESZ MI BOGACTWO SWE",
                "TO PRZEZ MOC W IMIENIU TWYM",
                "(: WSZYSTKO MOŻLIWE JEST :)"
            ]
        }
    ]
}