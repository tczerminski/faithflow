{
    "id": 375,
    "title": "OJCZE ŚWIATŁA",
    "verses": [
        {
            "lines": [
                "OJCZE ŚWIATŁA TY RADUJESZ SIĘ",
                "SWOIMI DZIEĆMI"
            ]
        },
        {
            "lines": [
                "WSZYSTKO CO DOBRE I DOSKONAŁE",
                "DZIEŁEM TWYM JEST"
            ]
        },
        {
            "lines": [
                "OJCZE ŚWIATŁA TY NIE ZMIENIASZ SIĘ",
                "W TOBIE NIE MA ZAĆMIENIA"
            ]
        }
    ]
}