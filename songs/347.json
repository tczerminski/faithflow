{
    "id": 347,
    "title": "O PANIE NASZ JAK WSPANIAŁE IMIĘ TWE",
    "verses": [
        {
            "lines": [
                "O PANIE NASZ!",
                "JAK WSPANIAŁE IMIĘ TWE",
                "OBJAWIA SIĘ!",
                "O BOŻE NASZ!",
                "CAŁA ZIEMIA PEŁNA CHWAŁY",
                "TWOJEJ JEST!"
            ]
        },
        {
            "lines": [
                "WIELBIMY, HOŁD CI ODDA JEMY",
                "SŁAWIMY ŚWIĘTE IMIĘ TWE!",
                "NIECH NASZA PIEŚŃ",
                "DLA CIEBIE BRZMI",
                "WIELBIMY IMIĘ TWE!"
            ]
        }
    ]
}