{
    "id": 498,
    "title": "ŚPIEWAĆ CHCĘ, PLĄSAĆ CHCĘ",
    "verses": [
        {
            "lines": [
                "ŚPIEWAĆ CHCĘ PLĄSAĆ CHCĘ",
                "BĘDĘ RADOWAĆ SIĘ W PANU MYM"
            ]
        },
        {
            "lines": [
                "ŚPIE WEM NASZYM PAN RADUJE SIĘ",
                "ON WCIĄŻ PRZEBYWAĆ CHCE",
                "W CHWAŁACH LUDU SWEGO",
                "ŚPIEW JEST TYM",
                "CO RADOŚĆ SPRAWIA MU",
                "(: DLATEGO PLĄSAĆ",
                "I ŚPIEWAĆ PANU CHCĘ :)"
            ]
        }
    ]
}