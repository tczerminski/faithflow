{
    "id": 35,
    "title": "BOŻE OJCZE JAK ŻE MOGŁEM",
    "verses": [
        {
            "lines": [
                "BOŻE OJCZE JAKŻE MOGŁEM",
                "PRZEZ TAK WIELE LAT BEZ CIEBIE",
                "ŻYĆ NA ZIEMI",
                "NIE POZNAJĄC MIŁOŚCI TWEJ!"
            ]
        },
        {
            "lines": [
                "LECZ TERAZ JESTEM",
                "TWOIM DZIECKIEM",
                "JUŻ NA ZAWSZE",
                "W TWEJ RODZINIE",
                "NIGDY WIĘC NIE BĘDĘ SAM",
                "BO TY MÓJ OJCZE JESTEŚ ZE MNĄ"
            ]
        },
        {
            "lines": [
                "PRAGNĘ CHWALIĆ CIEBIE",
                "ŚPIEWAĆ PIEŚNI CHWAŁY",
                "WIELBIĆ IMIĘ TWOJE DO KOŃCA DNI!"
            ]
        }
    ]
}