    "title": "PRZYJDŹ DO JEZUSA",
    "verses": [
        {
            "lines": [
                "PRZYJDŹ DO JEZUSA",
                "DAJ MU DZIŚ ŻYCIE SWE",
                "PRZYJDŹ DO JEZUSA",
                "POPROWADZI CIĘ"
            ]
        },
        {
            "lines": [
                "NIE MYŚL JUŻ",
                "JAK OGROMNY JEST GRZECH",
                "KTÓRY PRZYTŁACZA CIĘ",
                "DO PANA PRZYJDŹ – PRZEBACZY CI",
                "I POWIE JAK MASZ ŻYĆ"
            ]
        },
        {
            "lines": [
                "JUŻ NIE MOŻESZ ZWLEKAĆ NIE!",
                "TO JEST WŁAŚCIWY DZIEŃ!",
                "SKORZYSTAJ WIĘC Z SZANSY TEJ",
                "BO MOŻESZ SPÓŹNIĆ SIĘ"
            ]
        }
    ]
}