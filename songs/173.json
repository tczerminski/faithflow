{
    "id": 173,
    "title": "JAK JEST CUDOWNY OJCA DOM",
    "verses": [
        {
            "lines": [
                "JAK JEST CUDOWNY OJCA DOM",
                "DZISIAJ JESZCZE TEGO NIE WIEM,",
                "ALE W SŁOWA JEGO WIERZĘ",
                "CHCĘ KIEDYŚ Z TOBA MIESZKAĆ TAM",
                "I OGLĄDAĆ NOWE NIEBO",
                "NOWA ZIEMIĘ, WSZYSTKO NOWE"
            ]
        },
        {
            "lines": [
                "TAK! TYLKO DOTRZEĆ TAM",
                "TYLKO WYTRWAĆ W NIM",
                "Z WIARA W KOŃCU DNI",
                "ZNALEZIONYM BYĆ"
            ]
        },
        {
            "lines": [
                "CÓŻ NA ZIEMI ROBIĆ MAM?",
                "BĘDĘ MODLIĆ SIĘ DO CIEBIE,",
                "BĘDĘ MYŚLEĆ WCIĄŻ O TOBIE JEZU.",
                "CHCĘ TAK JAK DRZEWO OWOC DAĆ.",
                "OWOC MIŁY TOBIE PANIE,",
                "BY UWIELBIAĆ TWOJE IMIĘ.",
                "TAK!"
            ]
        }
    ]
}