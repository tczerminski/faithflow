{
    "id": 552,
    "title": "TWÓJ ŚWIĘTY ŻAR",
    "verses": [
        {
            "lines": [
                "TWÓJ ŚWIĘTY ŻAR NIECH",
                "SPADNIE NA NAS Z NIEBA",
                "TWÓJ ŚWIĘTY OGIEŃ",
                "NIECH PŁONIE W NAS",
                "TWÓJ ŚWIĘTY DUCH NIECH",
                "CIĄGLE NAS WYPEŁNIA",
                "SŁYSZYMY JUŻ ZWYCIĘZCÓW TWOICH KRZYK"
            ]
        },
        {
            "lines": [
                "TWÓJ ŚWIĘTY ŻAR NIECH SPADNIE TU",
                "CHCEMY WIĘCEJ CIEBIE WCIĄŻ",
                "NIECH PŁONIE W NAS WIDZIMY JUŻ",
                "PRZEBUDZONY NARÓD NASZ",
                "PRZYJDŹ DUCHU PRZYJDŹ",
                "PRZYJDŹ DUCHU PRZYJDŹ",
                "NIECH TWA MOC WYPEŁNI NAS",
                "PRAGNIEMY WCIĄŻ WIĘCEJ O TAK",
                "CHCEMY OGNIA TWEGO W NAS"
            ]
        }
    ]
}