    "verses": [
        {
            "lines": [
                "OTOM JA – PANIE MÓJ",
                "POKŁON CI ODDAJĘ",
                "W OBECNOŚCI TWEJ",
                "PRZEBYWAĆ CHCĘ"
            ]
        },
        {
            "lines": [
                "I NA PEWNO NIC NA ŚWIECIE",
                "NIE ROZŁĄCZY NAS",
                "JAK CUDOWNIE",
                "JEST WIELBIĆ PANIE CIĘ"
            ]
        }
    ]
