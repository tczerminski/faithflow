{
    "id": 282,
    "title": "MÓJ BÓG JEST WIELKI",
    "verses": [
        {
            "lines": [
                "MÓJ BÓG JEST WIELKI,",
                "ON PRZENOSI GÓRY,",
                "WSPIERA MNIE W DOLINIE,",
                "ZAWSZE PRZY MNIE JEST."
            ]
        },
        {
            "lines": [
                "MÓJ BÓG JEST WIELKI,",
                "LECZY MOJE RANY,",
                "WZMACNIA MOJE SIŁY",
                "NIE OPUŚCI MNIE."
            ]
        },
        {
            "lines": [
                "MÓJ BÓG JEST WIELKI",
                "WIELKI, WIELKI, WIELKI"
            ]
        },
        {
            "lines": [
                "MÓJ BÓG JEST WIELKI,",
                "ZBAWCA WSZYSTKICH LUDZI,",
                "PEŁNIA UZDROWIENIA",
                "PŁYNIE Z JEGO RAN."
            ]
        },
        {
            "lines": [
                "ON JEST CUDOWNY,",
                "WSZYSTKO MI WYBACZYŁ,",
                "DZIĘKI JEGO ŁASCE",
                "NOWE ŻYCIE MAM."
            ]
        },
        {
            "lines": [
                "(: POTĘŻNY :) X4",
                "WIELKI, WIELKI",
                "(: WSPANIAŁY :) X4",
                "WIELKI, WIELKI",
                "(: ŚWIĘTY :) X4",
                "WIELKI, WIELKI",
                "(: WSZECHMOGĄCY :) X4",
                "WIELKI, WIELKI"
            ]
        }
    ]
}