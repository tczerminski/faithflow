    "title": "W SERCACH NASZYCH",
    "verses": [
        {
            "lines": [
                "W SERCACH NASZYCH",
                "WZNIEŚMY PANU CZEŚĆ",
                "BO ON JEST GODZIEN BY GO CZCIĆ",
                "ŚPIEWAJ W SWYM DUCHU",
                "KLASZCZ W RĘCE SWE",
                "NASZ P AN JEST GODZIEN BY GO CZCIĆ!"
            ]
        },
        {
            "lines": [
                "GODZIEN! GODZIEN!",
                "ON JEST GODZIEN BY GO CZCIĆ!",
                "ALLELUJA! NASZ PAN JEST",
                "GODZIEN BY GO CZCIĆ!"
            ]
        }
    ]
}