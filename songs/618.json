{
    "id": 618,
    "title": "WIELE LAT BŁĄDZIŁEM",
    "verses": [
        {
            "lines": [
                "WIELE LAT BŁĄDZIŁEM",
                "W ŚWIECIE SAM",
                "NIC NIE DBAJĄC, ŻE NA KRZYŻU PAN",
                "ZA ME GRZECHY",
                "SWOJE ŻYCIE DAŁ NA GOLGOCIE"
            ]
        },
        {
            "lines": [
                "TAM ZA GRZECHY MOJE",
                "CHRYSTUS ZMARŁ",
                "TAM DOSTAŁEM DARMO",
                "ŻYCIA DAR",
                "TAM MIŁOŚCI MNIE",
                "OWINĄŁ ŻAR NA GOLGOCIE"
            ]
        },
        {
            "lines": [
                "BIBLIA MI ODKRYŁA",
                "GRZECHY ME",
                "KIEDY BYŁEM JUŻ NA PRZEPAŚCI DNIE",
                "WTEDY W SKRUCHY ŁZACH",
                "UDAŁEM SIĘ NA GOLGOTĘ"
            ]
        },
        {
            "lines": [
                "TERAZ WIEM",
                "ŻE JEZUS ZBAWIŁ MNIE",
                "KRÓLEM MOIM GO NAZYWAĆ ŚMIEM",
                "DUS ZA MOJA CIĄGLE ŚPIEWAĆ CHCE",
                "O GOLGOCIE"
            ]
        }
    ]
}