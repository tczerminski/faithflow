{
    "id": 258,
    "title": "KTÓŻ JEST BOGIEM, JEŚLI NIE NASZ PAN",
    "verses": [
        {
            "lines": [
                "KTÓŻ JEST BOGIEM",
                "JEŚLI NIE NASZ PAN?",
                "I KTÓŻ JEST TĄ SKAŁĄ",
                "JEŚLI NIE NASZ BÓG?"
            ]
        },
        {
            "lines": [
                "ON TWIERDZĄ NASZĄ JEST",
                "I PEWNYM WYBAWIENIEM",
                "DZIEŃ PO DNIU",
                "NA NOWO POMOC SWOJĄ ŚLE"
            ]
        },
        {
            "lines": [
                "OPOKĄ NASZĄ JEST",
                "ON TARCZĄ I WAROWNIĄ,",
                "KTÓRA WCIĄŻ",
                "OD WIEKÓW, AŻ NA WIEKI TRWA"
            ]
        }
    ]
}