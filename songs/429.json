{
    "id": 429,
    "title": "POSTANOWIŁEM IŚĆ ZA JEZUSEM",
    "verses": [
        {
            "lines": [
                "POSTANOWIŁEM IŚĆ ZA JEZUSEM",
                "(: NIE WRÓCĘ JUŻ! :)"
            ]
        },
        {
            "lines": [
                "CHOCIAŻ SAMOTNY TĄ DROGĄ IDĘ",
                "(: NIE WRÓCĘ JUŻ! :)"
            ]
        },
        {
            "lines": [
                "TEN ŚWIAT JEST ZA MNĄ",
                "A KRZYŻ PRZEDE MNĄ",
                "(: NIE WRÓCĘ JUŻ! :)"
            ]
        },
        {
            "lines": [
                "ACH ZABIERZ ŚWIAT TEN,",
                "A DAJ MI ZBAWCĘ",
                "(: NIE WRÓCĘ JUŻ! :)"
            ]
        },
        {
            "lines": [
                "NIKT NIE OBIECAŁ,",
                "ŻE BĘDZIE ŁATWO",
                "(: NIE WRÓCĘ JUŻ! :)"
            ]
        },
        {
            "lines": [
                "SZCZĘŚLIWY JESTEM,",
                "ŻE MAM JEZUSA",
                "(: NIE WRÓCĘ JUŻ! :)"
            ]
        },
        {
            "lines": [
                "BÓG NAM OBIECAŁ,",
                "(: ŻE BĘDZIE Z NAMI! :)"
            ]
        }
    ]
}