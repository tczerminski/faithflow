{
    "id": 32,
    "title": "BOŻE MÓJ PRZYNOSZĘ CI NA KOLANACH",
    "verses": [
        {
            "lines": [
                "BOŻE MÓJ PRZYNOSZĘ CI",
                "NA KOLANACH CAŁY NASZ KRAJ",
                "O! WYSŁUCHAĆ CHCIEJ",
                "MODLITWĘ TĘ",
                "I ZBAW OJCZYZNĘ MĄ"
            ]
        },
        {
            "lines": [
                "GDYŻ POLSKA POTRZEBUJE CIĘ",
                "POLACY TWYMI BYĆ CHCĄ",
                "O! PANIE ZWRÓĆ DZIŚ NA NAS",
                "OBLICZE SWE I OKAŻ ŁASKĘ SWĄ"
            ]
        },
        {
            "lines": [
                "O! WYSŁUCHAĆ CHCIEJ",
                "MODLITWĘ TĘ",
                "I ZBAW OJCZYZNĘ MĄ"
            ]
        }
    ]
}