{
    "id": 209,
    "title": "JEZU URATUJ NASZ KRAJ",
    "verses": [
        {
            "lines": [
                "JEZU! URATUJ NASZ KRAJ",
                "NIECH NIE BĘDZIE W NIM ZŁA",
                "TYLKO MIŁOŚĆ, MIŁOŚĆ TWA",
                "MY CHCEMY POMÓC CI W TYM",
                "SŁOWA WPROWADZIMY W CZYN",
                "TY ZAŚ PANIE",
                "PROWADŹ, PROWADŹ NAS"
            ]
        },
        {
            "lines": [
                "KRÓLUJ JEZU KRÓLUJ NAM",
                "W POLSKICH MIASTACH",
                "W POLSKICH WSIACH",
                "NIECH ZOBACZY CAŁY KRAJ",
                "JAK DOBRZE TO",
                "GDY KRÓLEM POLSKI JEST",
                "CHRYSTUS PAN"
            ]
        }
    ]
}