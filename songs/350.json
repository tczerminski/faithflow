{
    "id": 350,
    "title": "O PRZYJDŹCIE DO TRONU (JEŚLI CIERPISZ)",
    "verses": [
        {
            "lines": [
                "JEŚLI CIERPISZ I NIE MASZ JUŻ SIŁ",
                "DŹWIGAĆ WSZYSTKICH SWYCH GRZECHÓW I WIN",
                "JEZUS CIE WOŁA",
                "KIEDY NIE WIESZ JUŻ DOKĄD MASZ IŚĆ",
                "KIEDY PRAGNIESZ LECZ NIE MASZ CO PIĆ",
                "JEZUS CIĘ WOŁA"
            ]
        },
        {
            "lines": [
                "O PRZYJDŹCIE DO TRONU",
                "W RAMIONA OJCIEC WEŹMIE WAS",
                "WASZ DŁUG JEST SPŁACONY",
                "BO JEZUS WŁASNE ŻYCIE DAŁ"
            ]
        },
        {
            "lines": [
                "DZIŚ ZA SOBĄ ZOSTAWCIE SWÓJ WSTYD",
                "NIE ZWLEKAJCIE JUŻ CZAS ŻEBY PRZYJŚĆ",
                "JEZUS WAS WOŁA",
                "WASZE TROSKI ZAMIENIA NA ŚMIECH",
                "SWOJE ŻYCIE WAM WŁOŻY DO SERC",
                "JEZUS WAS WOŁA"
            ]
        },
        {
            "lines": [
                "TO NASZ ZBAWICIEL",
                "NIESAMOWITY BÓG",
                "O ALLELUJA JEZUS ŻYJE",
                "PRZED NIM SIĘ SKŁOŃMY",
                "JEDYNYM PANEM JEST",
                "O ALLELUJA JEZUS ŻYJE"
            ]
        },
        {
            "lines": [
                "NIEŚ SWÓJ KRZYŻ IDĄC DO NIEBA BRAM",
                "POWIEDZ ŚWIATU CO JEZUS CI DAŁ"
            ]
        }
    ]
}