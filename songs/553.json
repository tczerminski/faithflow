{
    "id": 553,
    "title": "TY BYŁEŚ PRZED STWORZENIEM",
    "verses": [
        {
            "lines": [
                "TY BYŁEŚ PRZED STWORZENIEM",
                "SAM UCZYNIŁEŚ TEN ŚWIAT",
                "TWE SŁOWO WPRAWIŁO W RUCH ZIEMIĘ",
                "A MI DAŁO ŻYĆ"
            ]
        },
        {
            "lines": [
                "TY BYŁEŚ KIEDY UPADŁEM",
                "WIDZIAŁEŚ MĄ WINĘ I WSTYD",
                "I Z NIMI UMARŁEŚ NA KRZYŻU",
                "A MI DAŁEŚ ŻYĆ"
            ]
        },
        {
            "lines": [
                "A JA NIE MAM NIC,",
                "CO MÓGŁBYM CI DAĆ",
                "PRÓCZ SERCA MOJEGO TOBIE,",
                "PANIE"
            ]
        },
        {
            "lines": [
                "CHCĘ STAĆ PRZED TOBĄ PODNOSZĄC RĘCE",
                "CHCĘ TRWAĆ",
                "WPATRUJĄC SIĘ W TWĄ TWARZ",
                "CHCĘ DAĆ CI CAŁE OTWARTE SERCE",
                "CHCĘ CIEBIE ZNAĆ"
            ]
        }
    ]
}