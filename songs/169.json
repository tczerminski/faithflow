{
    "id": 169,
    "title": "JAK DOBRZE RAZEM DZIŚ TU BYĆ",
    "verses": [
        {
            "lines": [
                "JAK DOBRZE RAZEM DZIŚ TU BYĆ",
                "W OBECNOŚCI TWEJ",
                "PRZED OBLICZE TWOJE PRZYJŚĆ",
                "MOC DUCHA SWEGO ZLEJ"
            ]
        },
        {
            "lines": [
                "TY IMIĘ MOJE ZNASZ",
                "I PANEM JESTEŚ MI",
                "WIĘC BĘDĘ WIELBIĆ IMIĘ TWE",
                "PO WSZYSTKIE DNI"
            ]
        },
        {
            "lines": [
                "TY KRÓLEM KRÓLÓW JESTEŚ ZWAN",
                "ŚWIĘTE JEST IMIĘ TWE",
                "ZWYCIĘZCĄ JEZUS MÓJ PAN",
                "ON MOCĄ DARZY MNIE"
            ]
        }
    ]
}