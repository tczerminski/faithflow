{
    "id": 630,
    "title": "WIĘKSZY JEST TEN",
    "verses": [
        {
            "lines": [
                "WIĘKSZY JEST TEN",
                "KTÓRY MIESZKA W NAS",
                "WIĘKSZY NIŻ TEN",
                "KTÓRY W ŚWIECIE JEST"
            ]
        },
        {
            "lines": [
                "BO JEZUS NASZ PAN",
                "NADZIEMSKIE WŁADZE",
                "ZWIERZCHNOŚCI ROZBROIŁ",
                "I WYSTAWIŁ JE NA POKAZ NAM",
                "ODNIÓSŁSZY NA KRZYŻU",
                "WIELKI TRYUMF NAD NIMI",
                "DZISIAJ PANEM",
                "I KRÓLEM KRÓLÓW JEST!"
            ]
        }
    ]
}