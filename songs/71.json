    "id": 71,
    "title": "CHWAŁA! CHWAŁA! BARANKOWI CZEŚĆ!",
    "verses": [
        {
            "lines": [
                "CHWAŁA! CHWAŁA!",
                "BARANKOWI CZEŚĆ!"
            ]
        },
        {
            "lines": [
                "ON GODZIEN CHWAŁY JEST",
                "I GODZIEN BY GO CZCIĆ",
                "NASZ PAN NA TRONIE SWYM",
                "WIELBIJMY GO!",
                "BO GODZIEN CHWAŁY JEST",
                "NASZ PAN NA TRONIE SWYM"
            ]
        }
    ]
}