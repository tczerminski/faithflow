{
    "id": 490,
    "title": "STWÓRCA WSZYSTKICH RZECZY",
    "verses": [
        {
            "lines": [
                "STWÓRCA WSZYSTKICH RZECZY,",
                "MALARZ FIRMAMENTU",
                "TO, CO NAJCENNIEJSZE PANIE DAŁEŚ MI"
            ]
        },
        {
            "lines": [
                "OD DNIA, GDY CIĘ SPOTKAŁEM",
                "I GDY POZNAJĘ MIŁOŚĆ TWĄ",
                "ME SERCE CORAZ BLIŻEJ",
                "CIEBIE PANIE JEST"
            ]
        },
        {
            "lines": [
                "PANIE MÓJ TYLKO CIEBIE WIELBIĆ CHCĘ,",
                "TWA MIŁOŚĆ CAŁKIEM ODMIENIŁA MNIE",
                "PANIE MÓJ TYLKO CIEBIE",
                "WIELBIĆ CHCĘ, ZA TO KIM NAPRAWDĘ JESTEŚ TY"
            ]
        },
        {
            "lines": [
                "STWÓRCA WSZYSTKICH RZECZY,",
                "MALARZ FIRMAMENTU",
                "PRZYJDZIE DZIEŃ,",
                "GDY UJRZĘ CIEBIE",
                "TWARZĄ W TWARZ"
            ]
        },
        {
            "lines": [
                "CHÓR ZASTĘPÓW ŚPIEWA",
                "O TYM JAK WIELKI JESTEŚ TY",
                "MIŁOŚĆ TWA WYPEŁNIA CAŁĄ DUSZĘ MĄ"
            ]
        }
    ]
}