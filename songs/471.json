{
    "id": 471,
    "title": "SIŁĄ MĄ JEST JEZUS",
    "verses": [
        {
            "lines": [
                "SIŁĄ MĄ JEST JEZUS",
                "ON TARCZĄ DLA MNIE JEST",
                "PROWADZI DO ZWYCIĘSTWA",
                "OPOKĄ JEST MÓJ PAN"
            ]
        },
        {
            "lines": [
                "CODZIENNIE MNIE POSILA",
                "ZE ŹRÓDŁA DAJE PIĆ",
                "SWYM SŁOWEM MNIE PROWADZI",
                "OPOKĄ JEST MÓJ PAN"
            ]
        },
        {
            "lines": [
                "OBIETNICE TWE PEWNE SĄ",
                "TWOJA WIERNOŚĆ NA WIEKI TRWA",
                "TY, O BOŻE NIE ZMIENIASZ SIĘ",
                "TYŚ JEST SKAŁĄ MĄ"
            ]
        }
    ]
}