{
    "id": 582,
    "title": "UDERZĄ NA MIASTO",
    "verses": [
        {
            "lines": [
                "UDERZĄ NA MIASTO",
                "ZDOBĘDĄ TEN MUR",
                "TAK LICZNE ZASTĘPY",
                "POSŁUSZNE MU SĄ"
            ]
        },
        {
            "lines": [
                "TO PAN WYDAJE SWÓJ GŁOS",
                "PRZED WOJSKIEM SWYM"
            ]
        },
        {
            "lines": [
                "DRUŻYNO SYJONU, SYJONU",
                "STAŃ RZĘDEM NA GÓRZE MEJ ŚWIĘTEJ",
                "DRUŻYNO SYJONU, SYJONU",
                "NA GÓRZE MEJ STAŃ!"
            ]
        }
    ]
}