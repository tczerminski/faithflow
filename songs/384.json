{
    "id": 384,
    "title": "OTO ON ŻYJE",
    "verses": [
        {
            "lines": [
                "OŚWIEĆ MNIE, OŚWIEĆ MNIE",
                "NIECH JEZUSA BLASK",
                "OŚWIECI MNIE"
            ]
        },
        {
            "lines": [
                "OCZYŚĆ MNIE, OCZYŚĆ MNIE",
                "NIECH JEZUSA KREW",
                "OCZYŚCI MNIE"
            ]
        },
        {
            "lines": [
                "PROWADŹ MNIE, PROWADŹ MNIE",
                "NIECH JEZUSA DUCH",
                "PROWADZI MNIE"
            ]
        }
    ]
}