{
    "id": 475,
    "title": "SKAŁĄ I ZBAWIENIEM… NADE MNĄ JEGO MIŁOŚĆ",
    "verses": [
        {
            "lines": [
                "(: SKAŁĄ I ZBAWIENIEM",
                "JEST MÓJ JEZUS,",
                "NADE MNĄ JEGO MIŁOŚĆ LŚNI :) X3",
                "SYN BOŻY, ZBAWICIEL, MÓJ PAN."
            ]
        },
        {
            "lines": [
                "(: JEDNA TYLKO DROGA DO POKOJU,",
                "PRZEZ ZBAWCY KREW",
                "I KRZYŻA MOC :) X3",
                "ON CHRONI, PROWADZI MNIE SAM."
            ]
        },
        {
            "lines": [
                "(: KOCHAM GO WIĘCEJ,",
                "Z KAŻDYM DNIEM,",
                "CHCĘ W JEGO SŁOWIE ZAWSZE TRWAĆ :) X3",
                "ON TOBIE SWĄ MIŁOŚĆ CHCE DAĆ"
            ]
        }
    ]
}