    "id": 3,
    "title": "ALLELUJA PANU CZEŚĆ",
    "verses": [
        {
            "lines": [
                "ALLELUJA PANU CZEŚĆ!",
                "CAŁA ZIEMIA JEST PEŁNA JEGO CZCI"
            ]
        },
        {
            "lines": [
                "PRZYCHODZIMY DO CIEBIE",
                "BOŻE NASZ Z SERCEM PEŁNYM",
                "MIŁOŚCI I WDZIĘCZNOŚCI",
                "I W POKORZE KŁANIAMY SIĘ",
                "PRZED KRÓLEM CHWAŁ",
                "NIECH BRZMI ALLELUJA",
                "PANU CZEŚĆ!"
            ]
        }
    ]
}