    "title": "BŁOGOSŁAWIEŃSTWO",
    "verses": [
        {
            "lines": [
                "NIECH CI BŁOGOSŁAWI PAN",
                "I NIECHAJ CIĘ STRZEŻE",
                "NIECH ROZJAŚNI PAN",
                "OBLICZE SWOJE NAD TOBĄ",
                "I NIECH MIŁOŚCIW CI BĘDZIE",
                "NIECH OBRÓCI PAN TWARZ SWOJĄ KU TOBIE",
                "I NIECH DA CI SWÓJ POKÓJ"
            ]
        }
    ]
