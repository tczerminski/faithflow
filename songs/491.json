{
    "id": 491,
    "title": "STWÓRCA ZIEMI",
    "verses": [
        {
            "lines": [
                "STWÓRCA ZIEMI I MILIONÓW GWIAZD",
                "NIESKOŃCZONY W MAJESTACIE PAN",
                "CAŁY WSZECHŚWIAT GŁOSI CHWAŁĘ TWĄ",
                "BOŻE CUDÓW PRZYJMIJ HOŁD"
            ]
        },
        {
            "lines": [
                "KIM JA JESTEM ŻE ZAPRASZASZ MNIE",
                "BYM ZAMIESZKAŁ W DOMU TWYM",
                "DAJESZ IMIĘ I DZIEDZICTWO SWE",
                "ROZKOSZ ŻYCIA W OBECNOŚCI TWEJ"
            ]
        },
        {
            "lines": [
                "WOŁAM: OJCZE, OJCZE",
                "WRACAM DZIŚ, PRZEBACZ MI",
                "W DOMU TWYM CHCĘ ŻYĆ",
                "WOŁAM: OJCZE, OJCZE",
                "JEDNO WIEM W SERCU MYM",
                "JESTEM SYNEM TWYM"
            ]
        },
        {
            "lines": [
                "ZNASZ MNIE LEPIEJ, NIŻ JA SIEBIE ZNAM",
                "W TWYCH OBJĘCIACH POKÓJ MÓJ",
                "NIE CHCĘ WIĘCEJ MOICH MAŁYCH DRÓG",
                "SKŁADAM WSZYSTKO U TWYCH STÓP"
            ]
        }
    ]
}