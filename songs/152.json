{
    "id": 152,
    "title": "I WRÓCĄ PRZEZ PANA KREW",
    "verses": [
        {
            "lines": [
                "I WRÓCĄ PRZEZ",
                "PANA KREW ODKUPIENI",
                "PRZYBĘDĄ Z OKRZYKIEM WESELA",
                "ZNIKNIE ZMARTWIENIE ŁZY I BÓL",
                "A RADOŚĆ OZDOBI IM TWARZ"
            ]
        },
        {
            "lines": [
                "BĘDĄ SIĘ ŚMIAĆ",
                "Z RADOŚCI W GŁOS",
                "ZMARTWIONY I SMUTNY",
                "NIE BĘDZIE NIKT"
            ]
        }
    ]
}