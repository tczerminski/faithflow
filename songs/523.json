{
    "id": 523,
    "title": "ŚWIĘTY… JEST NASZ BÓG",
    "verses": [
        {
            "lines": [
                "(: ŚWIĘTY, ŚWIĘTY, ŚWIĘTY",
                "JEST NASZ BÓG :)"
            ]
        },
        {
            "lines": [
                "NIEGASNĄCY PŁOMIEŃ, NIEUGASZONY ŻAR",
                "DOSKONAŁY W MIŁOŚCI, SPRAWIEDLIWY PAN",
                "BEZKRESNY OCEAN, NIEZMIERZONA DAL",
                "NIEPOJĘTY I ŚWIĘTY",
                "ADONAI"
            ]
        },
        {
            "lines": [
                "PRZED TOBĄ Z BOJAŹNIĄ STOIMY DZIŚ",
                "BO Z ŁASKI MOŻEMY ZBLIŻYĆ SIĘ",
                "PRZED TOBĄ KŁANIAMY SIĘ,",
                "WIELBIMY CIĘ"
            ]
        }
    ]
}