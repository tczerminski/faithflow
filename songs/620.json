{
    "id": 620,
    "title": "WIELKI JEST PAN I GODZIEN",
    "verses": [
        {
            "lines": [
                "WIELKI JEST PAN",
                "I GODZIEN WIELKIEJ CHWAŁY",
                "W MIEŚCIE BOGA NASZEGO",
                "I NA SWOJEJ GÓRZE ŚWIĘTEJ",
                "MALOWNICZO POŁOŻONA",
                "OZDOBĄ ZIEMI TEJ"
            ]
        },
        {
            "lines": [
                "JEST GÓRA SYJON",
                "NA KRAŃCACH PÓŁNOCNYCH",
                "MIASTO, GDZIE WIELKI JEST BÓG"
            ]
        },
        {
            "lines": [
                "WIELKI JEST PAN",
                "I GODZIEN WIELKIEJ CHWAŁY",
                "W SERCACH LUDU SWOJEGO",
                "I W KOŚCIELE JEGO ŚWIĘTYM",
                "ŚMIERCIĄ SWOJĄ GO ZBUDOWAŁ",
                "OZDOBIŁ CHWAŁĄ SWĄ"
            ]
        },
        {
            "lines": [
                "TO MIASTO ŚWIĘTE – NOWE JERUZALEM",
                "MIEJSCE, GDZIE NIE MA JUŻ ŁEZ"
            ]
        }
    ]
}