    "title": "NA WIEKI ŻYJĄCEMU",
    "verses": [
        {
            "lines": [
                "NA WIEKI ŻYJĄCEMU",
                "NIEŚMIERTELNEMU BOGU",
                "KRÓLOWI NIEWIDZIALNEMU",
                "JEDYNIE MĄDREMU, MĄDREMU BOGU"
            ]
        },
        {
            "lines": [
                "ODDAJMY CHWAŁĘ I CZEŚĆ KRÓLOWI",
                "TEMU CO ŻYJE NA WIEKI!",
                "ODDAJMY CHWAŁĘ I CZEŚĆ KRÓLOWI",
                "NA WIEKI! NA WIEKI! AMEN! AMEN!",
                "AMEN!"
            ]
        }
    ]
