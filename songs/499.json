{
    "id": 499,
    "title": "ŚPIEWAĆ CHCĘ, ŚPIEWAĆ CHCĘ",
    "verses": [
        {
            "lines": [
                "ŚPIEWAĆ CHCĘ, ŚPIEWAĆ CHCĘ,",
                "ŚPIEWAĆ CHCĘ ZAWSZE CHCĘ",
                "O DOBROCI, ŻYCZLIWOŚCI,",
                "MIŁOSIERDZIU TWYM ŚPIEWAĆ CHCĘ"
            ]
        },
        {
            "lines": [
                "ŚWIĘTY, ŚWIĘTY BARANKOWI CZEŚĆ",
                "CHWALĄC PIEŚNIĄ BOGA WSZELKIEJ CZCI",
                "ZAWSZE ŚPIEWAĆ CHCĘ"
            ]
        },
        {
            "lines": [
                "ŚPIEWAĆ PANU JUŻ OD RANA",
                "SZUKAĆ ŁASKI OD ŚWITANIA",
                "W MELODII, HARMONII",
                "ŚPIEWAĆ CHCĘ"
            ]
        },
        {
            "lines": [
                "POZWÓL NAM, POZWÓL NAM,",
                "POZWÓL NAM ŚPIEWAĆ CI",
                "O DOBROCI, ŻYCZLIWOŚCI,",
                "MIŁOSIERDZIU ŚPIEWAĆ CI"
            ]
        },
        {
            "lines": [
                "ŚPIEWAĆ CI, ŚPIEWAĆ CI,",
                "ŚPIEWAĆ CI CHCEMY DZIŚ",
                "O DOBROCI, ŻYCZLIWOŚCI,",
                "MIŁOSIERDZIU CHCEMY DZIŚ"
            ]
        }
    ]
}