{
    "id": 94,
    "title": "DOBRY DUCH PRZEBYWA POŚRÓD NAS",
    "verses": [
        {
            "lines": [
                "BOŻY DUCH PRZEBYWA POŚRÓD NAS",
                "WIEM, ŻE TO DUCH BOŻY",
                "WIEJE TCHNIENIEM SWYM",
                "ON ROZJAŚNIA TWARZE",
                "WSZYSTKIM W KRĄG",
                "TO WIDOCZNY BOŻEJ OBECNOŚCI ZNAK"
            ]
        },
        {
            "lines": [
                "O DUCHU ŚWIĘTY",
                "Z NIEBIAŃSKICH SFER",
                "POZOSTAŃ Z NAMI,",
                "WLEJ W NAS MIŁOŚĆ SWĄ",
                "ABY Z SERC PRZEPEŁNIONYCH",
                "PŁYNĄŁ CHWAŁY ŚPIEW",
                "NIKT Z NAS NIE WĄTPI JUŻ",
                "ŻE GDY WYJDZIEMY STĄD",
                "OŻYWIAĆ BĘDZIE NAS TWA MOC"
            ]
        },
        {
            "lines": [
                "BŁOGOSŁAWIEŃSTW W PEŁNI",
                "TY NIE MOŻESZ WZIĄĆ",
                "ZANIM NIE NAPEŁNI CIEBIE ŚWIĘTY DUCH",
                "JEŚLI CHCESZ ZWYCIĘSTWO",
                "W ŻYCIU ZAWSZE MIEĆ",
                "JEGO MOC NAPEŁNIAĆ MUSI SERCE TWE"
            ]
        },
        {
            "lines": [
                "CHOCIAŻ PAN",
                "PRZEBACZYŁ CI TWÓJ GRZECH",
                "JEDNAK SŁABY I ZWIĄZANY",
                "CZUJESZ SIĘ",
                "MOŻESZ WOLNYM BYĆ GDY PODDASZ SIĘ",
                "ABY PAN NAPEŁNIŁ CIĘ",
                "SWYM DUCHEM TEŻ"
            ]
        }
    ]
}