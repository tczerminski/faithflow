{
    "id": 669,
    "title": "ZEŚLIJ DESZCZ I WYLEJ SWEGO DUCHA",
    "verses": [
        {
            "lines": [
                "(: ZEŚLIJ DESZCZ :)",
                "I WYLEJ SWEGO DUCHA",
                "NA NASZ KRAJ",
                "(: ZEŚLIJ DESZCZ :)",
                "NAPEŁNIJ NAS SWĄ MOCĄ,",
                "ŚWIATŁEM SWYM",
                "ZEŚLIJ DESZCZ"
            ]
        },
        {
            "lines": [
                "NIECH PRZEPŁYWA",
                "STRUMIEŃ DUCHA",
                "PRZEZ WSZELKIE ZIEMSKIE CIAŁO",
                "I POTOKI WODY ŻYWEJ",
                "NIECH WYPŁYNĄ Z NAS"
            ]
        },
        {
            "lines": [
                "I NAPEŁNIJ SWOIM DUCHEM",
                "TWÓJ ŚWIĘTY NARÓD",
                "AŻ KAŻDY CZŁOWIEK",
                "PADNIE NA KOLANA",
                "PRZED PANEM SWYM"
            ]
        }
    ]
}