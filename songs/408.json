{
    "id": 408,
    "title": "PANIE NASZ, O PANIE NASZ",
    "verses": [
        {
            "lines": [
                "PANIE NASZ, O PANIE NASZ",
                "JAK PRZEDZIWNE TWE IMIĘ",
                "CAŁA ZIEMIA, CAŁY ŚWIAT",
                "TWĄ POTĘGĘ ZNA"
            ]
        },
        {
            "lines": [
                "MAŁE DZIECI WIELBIĄ CIĘ",
                "TWOIM WROGOM NA PRZEKÓR",
                "TWÓJ MAJESTAT WZNOSI SIĘ",
                "NAD NIEBIOSA"
            ]
        },
        {
            "lines": [
                "PATRZĘ NA NIEBO – RĄK TWOICH DZIEŁO",
                "KSIĘŻYC I GWIAZDY,",
                "KTÓRE STWORZYŁEŚ SAM",
                "CZYMŻE JEST CZŁOWIEK,",
                "ŻE O NIM PAMIĘTASZ.",
                "CZYM SYN CZŁOWIECZY,",
                "ŻE W OPIECE GO MASZ."
            ]
        },
        {
            "lines": [
                "NIEWIELE MNIEJSI",
                "OD ISTOT NIEBIESKICH",
                "WSZYSTKO ZŁOŻYŁEŚ POD STOPY NAM",
                "GDZIE NASZA CHWAŁA.",
                "GDZIE DOSTOJEŃSTWO.",
                "CO Z ZAUFANIEM,",
                "KTÓRYM DARZY NAS PAN"
            ]
        }
    ]
}