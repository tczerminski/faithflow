{
    "id": 162,
    "title": "JA WIDZĘ GO",
    "verses": [
        {
            "lines": [
                "JA WIDZĘ GO, WIDZĘ PANA CHWAŁ",
                "WYWYŻSZONEGO W UWIELBIENIU",
                "PRZEZ SWÓJ LUD NA ZIEMI TEJ",
                "TAK, WIDZĘ GO, MEGO PANA CHWAŁ",
                "ME OCZY WIDZĄ KRÓLA,",
                "KTÓRY ZASIADŁ NA SWÓJ TRON",
                "PANUJE NA WIEKI"
            ]
        },
        {
            "lines": [
                "JEDNYM TCHNIENIEM SWYM",
                "NAPEŁNIA WSZYSTKO",
                "CZCICIELI KRĄG OTACZA TRON",
                "BARANKA WŚRÓD CHWAŁ",
                "DOŁĄCZMY SIĘ ŚPIEWAJĄC",
                "ŚWIĘTY, ŚWIĘTY JEST NASZ PAN",
                "JEDYNY BÓG!"
            ]
        }
    ]
}