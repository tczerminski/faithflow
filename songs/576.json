{
    "id": 576,
    "title": "TYŚ RZEKŁ",
    "verses": [
        {
            "lines": [
                "TYŚ RZEKŁ PROŚ A JA DAM CI TO",
                "CO POTRZEBNE JEST",
                "TYŚ RZEKŁ –",
                "PROŚ A USŁYSZĘ Z NIEBA",
                "UZDROWIĘ TWÓJ KRAJ"
            ]
        },
        {
            "lines": [
                "TYŚ RZEKŁ, ŻE CHWAŁĄ WYPEŁNISZ ŚWIAT",
                "JAK MORZA TOŃ",
                "TYŚ RZEKŁ BYM PODNIÓSŁ SWÓJ WZROK",
                "BO ŻNIWA JEST CZAS",
                "KRÓLESTWO JEST TUŻ"
            ]
        },
        {
            "lines": [
                "TYŚ RZEKŁ – PROŚ BO JA",
                "CHCĘ NARODY CI DAĆ",
                "BO TO JEST PRAGNIENIEM",
                "MYM WCIĄŻ",
                "DOTKNĄĆ CHCĘ KRAŃCÓW ZIEMI",
                "A TAM MÓJ DUCH",
                "WNET ROZPALI SWÓJ ŻAR"
            ]
        },
        {
            "lines": [
                "O PANIE JA PROSZĘ O POLSKĘ"
            ]
        }
    ]
}