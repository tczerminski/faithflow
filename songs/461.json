{
    "id": 461,
    "title": "RANKIEM DNIA TRZECIEGO",
    "verses": [
        {
            "lines": [
                "RANKIEM DNIA TRZECIEGO",
                "TAK JAK JEZUS RZEKŁ",
                "POKONAŁ GRZECH, POKONAŁ ŚMIERĆ",
                "I Z GROBU POWSTAŁ SAM",
                "MAMY TERAZ NOWE ŻYCIE",
                "I KRÓLESTWO WIECZNE TRWA",
                "ALLELUJA! ALLELUJA!"
            ]
        },
        {
            "lines": [
                "ALLELUJA, JEZUS ŻYWY JEST",
                "ŚMIERCI MOC POKONAŁ",
                "I UWOLNIŁ Z GRZECHÓW MNIE",
                "JEZUS JUŻ NA WIEKI",
                "ŻYWY JEST, ŻYWY JEST!"
            ]
        },
        {
            "lines": [
                "ON JEST ALFĄ I OMEGĄ",
                "POCZĄTKIEM, KOŃCEM – ON",
                "ZAKON GRZECHU ZŁAMAŁ",
                "MAMY TERAZ WOLNOŚĆ W NIM",
                "BARANEK POWSTAŁ Z MARTWYCH",
                "ŻYWY JEST, ŻYWY JEST!"
            ]
        },
        {
            "lines": [
                "ON ZAPLANOWAŁ NASZE ŻYCIE",
                "AŻ PO KRES",
                "NAGROBNY KAMIEŃ PCHNĄŁ,",
                "FUNDAMENTEM NASZYM JEST",
                "ŚMIERĆ JUŻ NIE PANUJE,",
                "KARA OMINĘŁA NAS",
                "ALLELUJA! ALLELUJA!"
            ]
        },
        {
            "lines": [
                "CUDOWNYM DORADCĄ,",
                "WSZECHMOCNYM BOGIEM JEST",
                "ODWIECZNYM OJCEM I POKOJU KSIĘCIEM ON",
                "SŁOWEM, KTÓRE TRWA NA WIEKI,",
                "ŻYWY JEST! ŻYWY JEST!"
            ]
        }
    ]
}